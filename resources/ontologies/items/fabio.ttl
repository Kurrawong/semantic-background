    schema:name "has National Library of Medicine journal title abbreviation" ;
.

:hasNationalLibraryOfMedicineJournalId
    a
        owl:DatatypeProperty ,
        owl:FunctionalProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy ont: ;
    rdfs:range
        [
            a rdfs:Datatype ;
            owl:onDatatype xsd:string ;
            owl:withRestrictions (
                
                [
                    xsd:pattern "[0-9]+" ;
                ]
            ) ;
        ] ;
    rdfs:seeALso "http://www.nlm.nih.gov/" ;
    rdfs:subPropertyOf dcterms:identifier ;
    schema:description "An internal identifier for journals available from the National Library of Medicine repository." ;
    schema:name "has National Library of Medicine journal identifier" ;
.

:hasPII
    a owl:DatatypeProperty ;
