PREFIX : <http://purl.org/spar/fabio/>
PREFIX dc: <http://purl.org/dc/elements/1.1/>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX frbr: <http://purl.org/vocab/frbr/core#>
PREFIX ont: <http://purl.org/spar/fabio>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX prism: <http://prismstandard.org/namespaces/basic/2.0/>
PREFIX prismont: <http://prismstandard.org/namespaces/basic/2.0>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX vann: <http://purl.org/vocab/vann/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

prism:alternateTitle
    a owl:DatatypeProperty ;
    rdfs:isDefinedBy prismont: ;
    rdfs:subPropertyOf dcterms:title ;
    schema:description "An alternative title for a resource." ;
    schema:name "alternate title" ;
.

prism:doi
    a
        owl:DatatypeProperty ,
        owl:FunctionalProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy
        prismont: ,
        ont: ;
    rdfs:subPropertyOf dcterms:identifier ;
    schema:description "A Digital Object Identifier for a serial publication item, a dataset or some other entity." ;
    schema:name "has DOI" ;
.

prism:eIssn
    a
        owl:DatatypeProperty ,
        owl:FunctionalProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf dcterms:identifier ;
    schema:description "An International Standard Serial Number, ISSN, for electronic versions of serial publications." ;
    schema:name "has eISSN" ;
.

prism:edition
    a owl:DatatypeProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy prismont: ;
    schema:description "An identifier for one of several alternative issues of a magazine, a book or other resource." ;
    schema:name "has edition" ;
.

prism:endingPage
    a
        owl:DatatypeProperty ,
        owl:FunctionalProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy prismont: ;
    rdfs:subPropertyOf owl:topDataProperty ;
    schema:description "Identifies the last page of an entity such as a journal article." ;
    schema:name "has ending page" ;
.

prism:isbn
    a
        owl:DatatypeProperty ,
        owl:FunctionalProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy prismont: ;
    rdfs:subPropertyOf dcterms:identifier ;
    schema:description "The International Standard Book Number, ISBN, used to identify published books or, more rarely, journal issues." ;
    schema:name "has ISBN" ;
.

prism:issn
    a
        owl:DatatypeProperty ,
        owl:FunctionalProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy prismont: ;
    rdfs:subPropertyOf dcterms:identifier ;
    schema:description "The International Standard Serial Number, ISSN, an identifer for serial publications." ;
    schema:name "has ISSN" ;
.

prism:issueIdentifier
    a
        owl:DatatypeProperty ,
        owl:FunctionalProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy prismont: ;
    schema:description "An identifier used to identify a specific issue of a magazine, a journal or other resource." ;
    schema:name "has issue identifier" ;
.

prism:keyword
    a owl:DatatypeProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy prismont: ;
    rdfs:subPropertyOf owl:topDataProperty ;
    schema:description "A property used to tag a resource with a keyword that is likely to be used in search queries." ;
    schema:name "has keyword" ;
.

prism:pageRange
    a
        owl:DatatypeProperty ,
        owl:FunctionalProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy prismont: ;
    rdfs:subPropertyOf owl:topDataProperty ;
    schema:description "Identifies the page range of an entity such as a journal article." ;
    schema:name "has page range" ;
.

prism:publicationDate
    a
        owl:DatatypeProperty ,
        owl:FunctionalProperty ;
    dc:description """The date on which a document or entity is published.
The date can be expressed in three different formats:
- yyyy-mm-dd (i.e., xsd:date)
- yyyy-mm (i.e., xsd:gYearMonth)
- yyyy (i.e., xsd:gYear)""" ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy prismont: ;
    rdfs:range
        [
            a rdfs:Datatype ;
            owl:unionOf (
                xsd:date
                xsd:gYear
                xsd:gYearMonth
            ) ;
        ] ;
    rdfs:subPropertyOf dcterms:issued ;
    schema:description "The date on which a resource is published or disclosed." ;
    schema:name "has publication date" ;
.

prism:section
    a owl:DatatypeProperty ;
    rdfs:isDefinedBy prismont: ;
    rdfs:subPropertyOf owl:topDataProperty ;
    schema:description "The name or identifier of the section or part of a resource, for example Part 2 of Issue 6 of Volume 127 of the Journal of Cell Biology, or the Travel Section of a newspaper issue." ;
    schema:name "section" ;
.

prism:startingPage
    a
        owl:DatatypeProperty ,
        owl:FunctionalProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy prismont: ;
    rdfs:subPropertyOf owl:topDataProperty ;
    schema:description "Identifies the first page of an entity such as a journal article." ;
    schema:name "has starting page" ;
.

prism:versionIdentifier
    a owl:DatatypeProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy prismont: ;
    rdfs:subPropertyOf owl:topDataProperty ;
    schema:description "An identifier for a version of a resource, for example a text book, a draft of an article, or a data file." ;
    schema:name "has version identifier" ;
.

prism:volume
    a
        owl:DatatypeProperty ,
        owl:FunctionalProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy prismont: ;
    schema:description "An identifier for a particular volume of a resource, such as a journal or a multi-volume book." ;
    schema:name "has volume identifier" ;
.

prism:wordCount
    a
        owl:DatatypeProperty ,
        owl:FunctionalProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy prismont: ;
    rdfs:range xsd:positiveInteger ;
    rdfs:subPropertyOf owl:topDataProperty ;
    schema:description "The count of the number of words in a textual resource." ;
    schema:name "has word count" ;
.

:Addendum
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Expression ;
    schema:description "An item of material added at the end of a book or other publication, typically to include omitted or late-arriving material. " ;
    schema:name "addendum" ;
.

:Algorithm
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Specification ;
    schema:description "A precise sequential set of pre-defined logical rules or computational operations to be employed for solving a particular problem in a finite number of steps." ;
    schema:name "algorithm" ;
.

:ApplicationProfile
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Metadata ;
    schema:description "A set of metadata elements, policies and guidelines defined for a particular application.  The metadata elements used in the application profile may be drawn from more than one element sets, including locally defined sets. " ;
    schema:name "application profile" ;
.

:ApplicationProgrammingInterface
    a owl:Class ;
    rdfs:subClassOf :ComputerProgram ;
    schema:description " A computer program that enables a separate computer to interact programmatically with the computer running the API.  (Commonly abbreviated 'API'.)" ;
    schema:name "API" ;
    rdfs:isDefinedBy ont: ;
.

:ArchivalRecordSet
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:onProperty frbr:part ;
            owl:someValuesFrom :ArchivalRecord ;
        ] ,
        [
            a owl:Restriction ;
            owl:allValuesFrom :ArchivalRecord ;
            owl:onProperty frbr:part ;
        ] ,
        [
            a owl:Restriction ;
            owl:allValuesFrom :ArchivalDocumentSet ;
            owl:onProperty frbr:realization ;
        ] ,
        :WorkCollection ;
    schema:description "A collection of archival records." ;
    schema:name "archival record set" ;
.

:AudioDocument
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:onProperty frbr:realizationOf ;
            owl:someValuesFrom :SoundRecording ;
        ] ,
        :Expression ;
    schema:description "The realization of a sound recording." ;
    schema:name "audio document" ;
.

:BibliographicDatabase
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Database ;
    schema:description "A database providing an authoritative source of bibliographic information, for example PubMed (http://www.ncbi.nlm.nih.gov/pubmed), CrossRef Metadata Search (http://search.crossref.org/) and PubMed Central (http://www.ncbi.nlm.nih.gov/pmc/)." ;
    schema:name "bibliographic database" ;
.

:BibliographicMetadata
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Metadata ;
    schema:description "Standard bibliographic metadata describing an expression of a work.  To take the example of a journal article, bibliographic metadata typically include the authors' names, the date of publication, the title of the article, the journal name and volume number, the first and last page numbers, and the Digital Object Identifier (DOI)." ;
    schema:name "bibliographic metadata" ;
.

:Biography
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Work ;
    schema:description "An account of the events, works and achievements, both personal and professional, of a person, either living or dead." ;
    schema:name "biography" ;
.

:BlogPost
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf
        :Entry ,
        :WebContent ;
    schema:description "Information manifested in a blog, one of a set of periodic sequential entries containing commentary, descriptions of events, or other material such as images or videos, usually displayed in reverse-chronological order and usually maintained by an individual, or comments on such a post." ;
    schema:name "blog post" ;
.

:BookChapter
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:onProperty frbr:partOf ;
            owl:someValuesFrom :Book ;
        ] ,
        :Chapter ;
    schema:description "A defined chapter of a book, usually with a separate title or number." ;
    schema:name "book chapter" ;
.

:BookReview
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Review ;
    schema:description "A written review and critical analysis of the content, scope and quality of a book or other monographic work." ;
    schema:name "book review" ;
.

:BookSeries
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:onProperty frbr:part ;
            owl:someValuesFrom :Book ;
        ] ,
        :Series ;
    owl:disjointWith :BookSet ;
    schema:description "A sequence of books having certain characteristics in common that are formally identified together as a group - for instance, the books in the Law, Governance and Technology Series published by Springer." ;
    schema:name "book series" ;
.

:CallForApplications
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:onProperty frbr:realizationOf ;
            owl:someValuesFrom :Proposition ;
        ] ,
        :Expression ;
    schema:description "A document published by a funding agency requesting submission of applications for financial grants to fund projects, for example to enable research investigations in areas specified in the Call." ;
    schema:name "call for applications" ;
.

:CaseForSupportDocument
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:onProperty frbr:realizationOf ;
            owl:someValuesFrom :CaseForSupport ;
        ] ,
        :Expression ;
    schema:description "A document containing the case for support for a particular project, usually contained within a grant application document but sometimes distributed separately, without the financial and organizational information that the grant application document also contains." ;
    schema:name "case for support document" ;
.

:CitationMetadata
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Metadata ;
    schema:description "Metadata describing the citations made within a work to other works, and (optionally) some characteristics of the expressions of the cited works." ;
    schema:name "citation metadata" ;
.

:ClinicalCaseReport
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :CaseReport ;
    schema:description "A presentation of findings following a clinical or medical investigation on a human or animal patient, that may contain a diagnosis and proposals for therapeutic treatment and/or epidemiological control measures, or may propose further evaluative studies that will eventually lead to such outcomes." ;
    schema:name "clinical case report" ;
.

:ClinicalGuideline
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :InstructionalWork ;
    schema:description "A recommendation on the appropriate treatment and care of people with a specific disease or condition, based on the best available evidence, designed to help healthcare professionals in their work." ;
    schema:name "clinical guideline" ;
.

:ClinicalTrialDesign
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Specification ;
    schema:description "A predefined written procedural method, designed to ensure reliability of findings, for undertaking a medical or veterinary clinical study of the safety, efficacy, or optimum dosage schedule of one or more diagnostic, therapeutic or prophylactic drugs or treatments, or of devices or techniques, involving a randomized controlled trial for evidence-based assessment in humans or animals, specifying criteria of eligibility, nature of controls, sampling schedules, data collection parameters, statistical analyses, reporting standards, etc. to be employed in undertaking the clinical trial." ;
    schema:name "clinical trial design" ;
.

:ClinicalTrialReport
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :TrialReport ;
    schema:description "The report of a pre-planned medical or veterinary clinical study of the safety, efficacy, or optimum dosage schedule of one or more diagnostic, therapeutic or prophylactic drugs, or of devices, treatments or techniques, involving a randomized controlled trial for evidence-based assessment in humans or animals selected according to predetermined criteria of eligibility and observed for evidence of favourable and unfavourable effects." ;
    schema:name "clinical trial report" ;
.

:CompleteWorks
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :CollectedWorks ;
    schema:description "A collection of all the literary or scholastic works of a single person." ;
    schema:name "complete works" ;
.

:ComputerApplication
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :ComputerProgram ;
    schema:description "A computer program designed to assist a human user to perform one or more goal-oriented tasks such as word processing or image processing.  A computer application will typically save its output files in one or more specific formats, conforming either to proprietary or open standards.  " ;
    schema:name "computer application" ;
.

:ComputerFile
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :DigitalItem ;
    schema:description "A digital item containing information in computer-readable form encoded in a particular format." ;
    schema:name "computer file" ;
.

:ConferencePoster
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Expression ;
    schema:description "A display poster, typically containing text with illustrative figures and/or tables, usually reporting research results or proposing hypotheses, submitted for acceptance to and/or presented at a conference, seminar, symposium, workshop or similar event." ;
    schema:name "conference poster" ;
.

:ConferenceProceedings
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:onProperty frbr:part ;
            owl:someValuesFrom :ConferencePaper ;
        ] ,
        :AcademicProceedings ;
    schema:description "A document containing the programme and collected conference papers, or their abstracts, presented at a conference, seminar, symposium or similar event." ;
    schema:name "conference proceedings" ;
.

:Corrigendum
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Correction ;
    schema:description "A formal correction to an error introduced by the author into a previously published document." ;
    schema:name "corrigendum" ;
.

:Cover
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Expression ;
    schema:description "A protective covering used to bind together the pages of a document or the first, informative, page of a digital document." ;
    schema:name "cover" ;
.

:CriticalEdition
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Work ;
    schema:description "A new edition of a historical publication, edited by a scholar other than the original author, containing within the body text the supposedly best version of the original work, with footnotes detailing and commenting on textual variations between different versions, typically with an introduction to the original work written by the scholar, and with a bibliography listing related publications." ;
    schema:name "critical edition" ;
.

:DataFile
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:onProperty frbr:realizationOf ;
            owl:someValuesFrom :Dataset ;
        ] ,
        :Expression ;
    schema:description "A realisation of a fabio:Dataset (a frbr:Work) containing a defined collection of data with specific content and possibly with a specific version number, that can be embodied as a fabio:Digital Manifestation (a frbr:Manifestation with a specific format) and be represented by a specific fabio:ComputerFile (a frbr:Item) on someone's hard drive." ;
    schema:name "data file" ;
.

:DataManagementPlan
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:onProperty frbr:realizationOf ;
            owl:someValuesFrom :Proposition ;
        ] ,
        :Expression ;
    schema:description "A structured document giving information about how data arising from a research project or other endeavour is to be manages, preserved and shared." ;
    schema:name "data management plan" ;
.

:DataManagementPolicy
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Policy ;
    schema:description "A policy that descibes and defines how data should be managed, preserved and shared." ;
    schema:name "data management policy" ;
.

:DataManagementPolicyDocument
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :PolicyDocument ;
    schema:description "A document embodying a policy that descibes and defines how data should be managed, preserved and shared." ;
    schema:name "data management policy document" ;
.

:DataRepository
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Repository ;
    schema:description "A repository for storing data." ;
    schema:name "data repository" ;
.

:DatabaseManagementSystem
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :ComputerProgram ;
    schema:description "The software used to create a database.  (Commonly abbreviated 'DBMS'.)" ;
    schema:name "database management system" ;
.

:DefinitiveVersion
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Manuscript ;
    schema:description "The final published expression of a work that bears the publisher's imprimatur. Typically for a journal article, the Definitive Version results from revision of an earlier submitted version of the work following peer review, and is then published in print and/or digital form after the publisher has assigned it a DOI.  The Definitive Version is also known as the Version of Record, although according to the CrossRef Glossary (http://crossref.org/02publishers/glossary.html) that term can also refer to the author's final version of a work that is not formally published. " ;
    schema:name "definitive version" ;
.

:DemoPaper
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Expression ;
    schema:description "A demonstration paper, typically describing a new product, service or system created as a result of research, usually presented during a conference or workshop." ;
    schema:name "demo paper" ;
.

:Diary
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:onProperty frbr:part ;
            owl:someValuesFrom :Entry ;
        ] ,
        :Notebook ;
    schema:description "A personal record, in a form of book, with discrete entries (often handwritten) arranged by date, reporting what has happened over the course of a day or other period of time." ;
    schema:name "diary" ;
.

:Directory
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Database ;
    schema:description "A database of information which is heavily optimized for reading." ;
    schema:name "directory" ;
.

:DocumentRepository
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Repository ;
    schema:description "A repository for storing documents." ;
    schema:name "document repository" ;
.

:DustJacket
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Expression ;
    schema:description "A detachable outer cover, usually made of paper and printed with text and illustrations. This outer cover has folded flaps that hold it to the cover of a document." ;
    schema:name "dust jacket" ;
.

:Email
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Expression ;
    schema:description "A message transmitted over the internet as an item of electronic mail, typically based on the Simple Mail Transfer Protocol (SMTP).  Emails can have computer files containing documents, dataset and images attached to them or embedded within them." ;
    schema:name "e-mail" ;
.

:EntityMetadata
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Metadata ;
    schema:description "Metadata describing the work itself, including for example the name of the creator(s), the title of the work, and the date and place of its creation." ;
    schema:name "entity metadata" ;
.

:Erratum
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Correction ;
    schema:description "A formal correction to an error introduced by the publisher into a previously published document." ;
    schema:name "erratum" ;
.

:Essay
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Work ;
    schema:description "A piece of non-fiction writing on a particular subject, usually of moderate length and without chapters." ;
    schema:name "essay" ;
.

:ExaminationPaper
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Work ;
    schema:description "A set of questions on a particular topic designed to test the academic, professional or technical ability of the person taking the examination, with achievement of a pass grade in the examination typically being a prerequisite for the award of an educational award such as a degree, or of a professional or technical qualification." ;
    schema:name "examination paper" ;
.

:ExecutiveSummary
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :BriefReport ;
    schema:description "An executive summary is a brief report summarizing a longer formal report, designed to present the key points, conclusions and recommendations arising from the study being reported, for readers too busy to take the time to read the complete report." ;
    schema:name "executive summary" ;
.

:ExperimentalProtocol
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Specification ;
    schema:description "A predefined written procedural method, designed to ensure successful replication of results by others in the same or other laboratories, that describes the overall objectives, organization and implementation of a scientific experiment, and specifies the experimental design, experimental methods, reagents, instrumentation, sampling schedules, data collection parameters, statistical analyses, image processing procedures, safety precautions, reporting standards, etc. employed in undertaking the experiment." ;
    schema:name "experimental protocol" ;
.

:Figure
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:onProperty frbr:realizationOf ;
            owl:someValuesFrom :StillImage ;
        ] ,
        :Expression ;
    schema:description "A visual communication object comprising one or more still images on a related theme.  If included within a publication, a figure is typically unaligned with the main body of text, having its own descriptive textual figure legend." ;
    schema:name "figure" ;
.

:Film
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Movie ;
    schema:description "A movie with an accompanying soundtrack, typically created by a professional film studio, designed to communicate a fictional story, record an artistic event, or impart information that is scientific or documentary in nature." ;
    schema:name "film" ;
.

:Folksonomy
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :UncontrolledVocabulary ;
    schema:description "A system of classification derived from the practice and method of collaboratively creating and managing tags to annotate and categorize content in a particular domain. [Contrast fabio:Ontology]" ;
    schema:name "folksonomy" ;
.

:GanttChart
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Expression ;
    schema:description "A horizontal bar chart used to guide project planning, execution and control, illustrating the project schedule, with a separate line indicating the start and end dates of each of the key project activities or workpackages, and optionally showing the dependencies between these items. A Gantt chart is typically part of a project plan." ;
    schema:name "Gantt chart" ;
.

:GrantApplicationDocument
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:onProperty frbr:realizationOf ;
            owl:someValuesFrom :GrantApplication ;
        ] ,
        :Expression ;
    schema:description "The realization of a grant application, usually containing a case for support document." ;
    schema:name "grant application document" ;
.

:Hardback
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :PrintObject ;
    schema:description "A print object bound with rigid protective covers (typically of cardboard covered with cloth, heavy paper, or sometimes leather)." ;
    schema:name "hardback" ;
.

:InBrief
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            owl:unionOf (
                :JournalNewsItem
