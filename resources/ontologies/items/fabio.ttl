        ] ,
        frbr:Manifestation ;
    schema:description """A subclass of FRBR manifestation, restricted to manifestations of fabio:Expressions. fabio:Manifestation specifically applies to electronic (digital) as well as to physical manifestations of expressions.

Examples of different manifestations of a single 'version of record' expression of a scholarly work include an article in a print journal or the on-line version of that article as a web page.""" ;
    schema:name "manifestation" ;
.

:Report
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Work ;
    schema:description "A formal factual, methodological, statistical, technical or research report issued by an individual, group, agency, government body or other institution." ;
    schema:name "report" ;
.

frbr:Item
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/vocab/frbr/core> ;
    schema:description "Item" ;
    schema:name "Item" ;
.

frbr:Work
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/vocab/frbr/core> ;
    schema:description "Work" ;
    schema:name "Work" ;
.

:Item
    a owl:Class ;
    dc:description "A fabio:Item can only have part or be part of another fabio:Item. Moreover, it can be an exemplar only of a fabio:Manifestation." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            owl:intersectionOf (
                
                [
                    a owl:Restriction ;
                    owl:allValuesFrom frbr:Item ;
                    owl:onProperty frbr:part ;
                ]
                
                [
                    a owl:Restriction ;
                    owl:allValuesFrom frbr:Item ;
                    owl:onProperty frbr:partOf ;
                ]
            ) ;
        ] ,
        [
            a owl:Restriction ;
            owl:allValuesFrom :Manifestation ;
            owl:onProperty frbr:exemplarOf ;
        ] ,
        frbr:Item ;
    schema:description "A subclass of FRBR item, restricted to exemplars of fabio:Manifestations.  An example of a fabio:Item is a printed copy of a journal article on your desk, or a PDF file of that article that you purchased from a publisher and that now resides in digital form on your computer hard drive.  " ;
    schema:name "item" ;
.

:DigitalStorageMedium
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:allValuesFrom :DigitalItem ;
            owl:onProperty :stores ;
        ] ,
        :StorageMedium ;
    schema:description "A means of storing information in digital form, involving binary encoding of data in 0s and 1s, e.g. a computer random access memory, hard disc, USB stick, CD, DVD or digital magnetic tape." ;
    schema:name "digital storage medium" ;
.

:Specification
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Work ;
    schema:description "An explicit description of, or set of requirements to be satisfied by, a material, product, resource, service or standard." ;
    schema:name "specification" ;
.


frbr:partOf
    a owl:ObjectProperty ;
    rdfs:isDefinedBy <http://purl.org/vocab/frbr/core> ;
    schema:description "Meaning = is part of.  As defined by FRBR (http://www.ifla.org/VII/s13/frbr/frbr1.htm; http://purl.org/vocab/frbr/core#partOf), a property indicating that one entity is a part of another entity." ;
    schema:name "is part of" ;
.

frbr:part
    a owl:ObjectProperty ;
    rdfs:isDefinedBy <http://purl.org/vocab/frbr/core> ;
    schema:description "Meaning = has part.  As defined by FRBR (http://www.ifla.org/VII/s13/frbr/frbr1.htm; http://purl.org/vocab/frbr/core#part),  a property indicating that one entity includes another entity." ;
    schema:name "has part" ;
.

frbr:realizationOf
    a owl:ObjectProperty ;
    rdfs:isDefinedBy <http://purl.org/vocab/frbr/core> ;
    schema:description "As defined by FRBR (http://www.ifla.org/VII/s13/frbr/frbr1.htm; http://purl.org/vocab/frbr/core#realizationOf), a property representing the work that has been realized by an expression." ;
    schema:name "is realization of" ;
.

:Work
    a owl:Class ;
    dc:description "A fabio:Work can only have part or be part of another fabio:Work. Moreover, it can be realized only by fabio:Expression(s)." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:allValuesFrom :Expression ;
            owl:onProperty frbr:realization ;
        ] ,
        [
            owl:intersectionOf (
                
                [
                    a owl:Restriction ;
                    owl:allValuesFrom frbr:Work ;
                    owl:onProperty frbr:part ;
                ]
                
                [
                    a owl:Restriction ;
                    owl:allValuesFrom frbr:Work ;
                    owl:onProperty frbr:partOf ;
                ]
            ) ;
        ] ,
        frbr:Work ;
    schema:description "A subclass of FRBR work, restricted to works that are published or potentially publishable, and that contain or are referred to by bibliographic references, or entities used to define bibliographic references. FaBiO works, and their expressions and manifestations, are primarily textual publications such as books, magazines, newspapers and journals, and items of their content.  However, they also include datasets, computer algorithms, experimental protocols, formal specifications and vocabularies, legal records, governmental papers, technical and commercial reports and similar publications, and also bibliographies, reference lists, library catalogues and similar collections. For this reason, fabio:Work is not an equivalent class to frbr:ScholarlyWork.  An example of a fabio:Work is your latest research paper." ;
    schema:name "work" ;
.

frbr:Endeavour
    a owl:Class ;
    rdfs:isDefinedBy <http://purl.org/vocab/frbr/core> ;
    schema:description "Endeavour" ;
    schema:name "Endeavour" ;
.

:Expression
    a owl:Class ;
    dc:description "A fabio:Expression can only have part or be part of another fabio:Expression.  Moreover, it can be a representation only of a fabio:Work, and  it can be embodied only in fabio:Manifestation(s)." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:allValuesFrom :Manifestation ;
            owl:onProperty frbr:embodiment ;
        ] ,
        [
            a owl:Restriction ;
            owl:allValuesFrom :Work ;
            owl:onProperty frbr:realizationOf ;
        ] ,
        [
            owl:intersectionOf (
                
                [
                    a owl:Restriction ;
                    owl:allValuesFrom frbr:Expression ;
                    owl:onProperty frbr:part ;
                ]
                
                [
                    a owl:Restriction ;
                    owl:allValuesFrom frbr:Expression ;
                    owl:onProperty frbr:partOf ;
                ]
            ) ;
        ] ,
        frbr:Expression ;
    schema:description "A subclass of FRBR expression, restricted to expressions of fabio:Works.  For your latest research paper, the preprint submitted to the publisher, and the final published version to which the publisher assigned a unique digital object identifier, are both expressions of the same work.  " ;
    schema:name "expression" ;
.

