    rdfs:subPropertyOf dcterms:identifier ;
    schema:description "Has Publisher Item Identifier" ;
    schema:name "has PII" ;
.

:hasPageCount
    a
        owl:DatatypeProperty ,
        owl:FunctionalProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy ont: ;
    rdfs:range xsd:positiveInteger ;
    rdfs:subPropertyOf owl:topDataProperty ;
    schema:description "The count of the number of pages in a textual resource." ;
    schema:name "has page count" ;
.

:hasPatentNumber
    a owl:DatatypeProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf dcterms:identifier ;
    schema:description "A unique identifing number issued by a patent authority to identify a patent, displayed at the beginning of the patent document." ;
