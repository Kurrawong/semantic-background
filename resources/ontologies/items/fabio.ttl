.

:hasPlaceOfPublication
    a owl:ObjectProperty ;
    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy ont: ;
    rdfs:range frbr:Place ;
    schema:description "The place (usually, the city) where the publisher of a particular bibliographic resource is located." ;
    schema:name "has place of publication" ;
.

:hasPortrayal
    a owl:ObjectProperty ;
