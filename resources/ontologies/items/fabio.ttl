    rdfs:domain frbr:Endeavour ;
    rdfs:isDefinedBy ont: ;
    rdfs:range xsd:string ;
    rdfs:seeAlso "http://www.ncbi.nlm.nih.gov/pubmed/" ;
    rdfs:subPropertyOf dcterms:identifier ;
    schema:description "An identifier for bibliographic records held by the PubMed repository." ;
    schema:name "has PubMed identifier" ;
.

:hasPublicationYear
    a
        owl:DatatypeProperty ,
        owl:FunctionalProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:range xsd:gYear ;
    rdfs:subPropertyOf dcterms:issued ;
    schema:description "The year in which a resource is published." ;
    schema:name "has publication year" ;
.

:hasRepresentation
    a owl:ObjectProperty ;
