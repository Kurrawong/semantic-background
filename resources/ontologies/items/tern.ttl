tern:Boolean
    a
        owl:Class ,
        sh:NodeShape ;
    schema:description "Class to encapsulate a true-or-false value" ;
    schema:name "Boolean" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:allValuesFrom xsd:boolean ;
            owl:onProperty rdf:value ;
        ] ,
        [
            a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty rdf:value ;
        ] ,
        tern:Value ;
    sh:property
        [
            a sh:PropertyShape ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path rdf:value ;
        ] ;
.

