        "http://web.mit.edu/2.25/www/pdf/DA_unified.pdf"^^xsd:anyURI ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:cardinality "1"^^xsd:int ;
            owl:onProperty qudt:vectorMagnitude ;
        ] ,
        [
            a owl:Restriction ;
            owl:allValuesFrom xsd:float ;
            owl:onProperty qudt:vectorMagnitude ;
        ] ,
        [
            a owl:Restriction ;
            owl:cardinality "1"^^xsd:int ;
            owl:onProperty qudt:hasBaseQuantityKind ;
        ] ,
        [
            a owl:Restriction ;
            owl:allValuesFrom qudt:QuantityKind ;
            owl:onProperty qudt:hasBaseQuantityKind ;
        ] ,
        qudt:Concept ;
    schema:description """A _Dimension_ expresses a magnitude for a base quantity kind such as mass, length and time.

DEPRECATED - each exponent is expressed as a property. Keep until a validation of this has been done.""" ;
    schema:name "Base Dimension Magnitude" ;
.

