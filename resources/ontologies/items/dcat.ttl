PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX dctype: <http://purl.org/dc/dcmitype/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX sdo: <http://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX vcard: <http://www.w3.org/2006/vcard/ns#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

dcat:accessURL
    a owl:ObjectProperty ;
    rdfs:domain dcat:Distribution ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range rdfs:Resource ;
    owl:propertyChainAxiom (
        dcat:accessService
        dcat:endpointURL
    ) ;
    skos:altLabel "adgangsURL"@da ;
    skos:editorialNote "Status: English Definition text modified by DCAT revision team, updated Italian and Czech translation provided, translations for other languages pending." ;
    skos:scopeNote "If the distribution(s) are accessible only through a landing page (i.e. direct download URLs are not known), then the landing page link should be duplicated as accessURL on a distribution." ;
    schema:description "A URL of a resource that gives access to a distribution of the dataset. E.g. landing page, feed, SPARQL endpoint. Use for all cases except a simple download link, in which case downloadURL is preferred." ;
    schema:name "access address" ;
.

dcat:bbox
    a owl:DatatypeProperty ;
    rdfs:domain dct:Location ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range rdfs:Literal ;
    skos:changeNote "New property added in DCAT 2.0." ;
    skos:scopeNote "The range of this property is intentionally generic, with the purpose of allowing different geometry encodings. E.g., the geometry could be encoded with as WKT (geosparql:wktLiteral [GeoSPARQL]) or [GML] (geosparql:asGML [GeoSPARQL])." ;
    schema:description "The geographic bounding box of a resource." ;
    schema:name "bounding box" ;
.

dcat:byteSize
    a owl:DatatypeProperty ;
    rdfs:domain dcat:Distribution ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range rdfs:Literal ;
    skos:scopeNote "The size in bytes can be approximated when the precise size is not known. The literal value of dcat:byteSize should by typed as xsd:decimal." ;
    schema:description "The size of a distribution in bytes." ;
    schema:name "byte size" ;
.

dcat:catalog
    a owl:ObjectProperty ;
    rdfs:domain dcat:Catalog ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range dcat:Catalog ;
    rdfs:subPropertyOf
        dct:hasPart ,
        rdfs:member ;
    skos:altLabel "har delkatalog"@da ;
    skos:changeNote "New property added in DCAT 2.0." ;
    schema:description "A catalog whose contents are of interest in the context of this catalog." ;
    schema:name "catalog" ;
.

dcat:centroid
    a owl:DatatypeProperty ;
    rdfs:domain dct:Location ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range rdfs:Literal ;
    skos:altLabel "centroide"@da ;
    skos:changeNote "New property added in DCAT 2.0." ;
    skos:scopeNote "The range of this property is intentionally generic, with the purpose of allowing different geometry encodings. E.g., the geometry could be encoded with as WKT (geosparql:wktLiteral [GeoSPARQL]) or [GML] (geosparql:asGML [GeoSPARQL])." ;
    schema:description "The geographic center (centroid) of a resource." ;
    schema:name "centroid" ;
.

dcat:compressFormat
    a owl:ObjectProperty ;
    rdfs:domain dcat:Distribution ;
    rdfs:isDefinedBy <https://www.w3.org/TR/vocab-dcat-2/> ;
    rdfs:range dct:MediaType ;
    rdfs:subPropertyOf dct:format ;
    skos:changeNote "New property added in DCAT 2.0." ;
    skos:scopeNote "This property is to be used when the files in the distribution are compressed, e.g. in a ZIP file. The format SHOULD be expressed using a media type as defined by IANA media types registry https://www.iana.org/assignments/media-types/, if available." ;
    schema:description "The compression format of the distribution in which the data is contained in a compressed form, e.g. to reduce the size of the downloadable file." ;
    schema:name "compression format" ;
.

dcat:contactPoint
    a owl:ObjectProperty ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range vcard:Kind ;
    skos:editorialNote "Status: English Definition text modified by DCAT revision team, Italian, Spanish and Czech translations provided, other translations pending." ;
    schema:description "Relevant contact information for the catalogued resource. Use of vCard is recommended." ;
    schema:name "contact point" ;
.

dcat:dataset
    a owl:ObjectProperty ;
    rdfs:domain dcat:Catalog ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range dcat:Dataset ;
    rdfs:subPropertyOf
        dct:hasPart ,
        rdfs:member ;
    skos:altLabel "har datasæt"@da ;
    skos:editorialNote "Status: English Definition text modified by DCAT revision team, Italian, Spanish and Czech translation provided, other translations pending." ;
    schema:description "A collection of data that is listed in the catalog." ;
    schema:name "dataset" ;
.

dcat:distribution
    a owl:ObjectProperty ;
    rdfs:domain dcat:Dataset ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range dcat:Distribution ;
    rdfs:subPropertyOf dct:relation ;
    skos:altLabel "har distribution"@da ;
    skos:editorialNote "Status: English Definition text modified by DCAT revision team, translations pending (except for Italian, Spanish and Czech)." ;
    schema:description "An available distribution of the dataset." ;
    schema:name "distribution" ;
.

dcat:downloadURL
    a owl:ObjectProperty ;
    rdfs:domain dcat:Distribution ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range rdfs:Resource ;
    skos:editorialNote "Status: English  Definition text modified by DCAT revision team, Italian, Spanish and Czech translation updated, other translations pending." ;
    skos:scopeNote "dcat:downloadURL SHOULD be used for the address at which this distribution is available directly, typically through a HTTP Get request." ;
    schema:description "The URL of the downloadable file in a given format. E.g. CSV file or RDF file. The format is indicated by the distribution's dct:format and/or dcat:mediaType." ;
    schema:name "download URL" ;
.

dcat:endDate
    a owl:DatatypeProperty ;
    rdfs:domain dct:PeriodOfTime ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range rdfs:Literal ;
    skos:changeNote "New property added in DCAT 2.0." ;
    skos:scopeNote "The range of this property is intentionally generic, with the purpose of allowing different level of temporal precision for specifying the end of a period. E.g., it can be expressed with a date (xsd:date), a date and time (xsd:dateTime), or a year (xsd:gYear)." ;
    schema:description "The end of the period." ;
    schema:name "end date" ;
.

dcat:endpointDescription
    a owl:ObjectProperty ;
    rdfs:domain dcat:DataService ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    skos:changeNote
        "New property in DCAT 2.0." ,
        "Nueva propiedad agregada en DCAT 2.0." ;
    skos:scopeNote
        "An endpoint description may be expressed in a machine-readable form, such as an OpenAPI (Swagger) description, an OGC GetCapabilities response, a SPARQL Service Description, an OpenSearch or WSDL document, a Hydra API description, else in text or some other informal mode if a formal representation is not possible." ,
        "The endpoint description gives specific details of the actual endpoint instance, while dct:conformsTo is used to indicate the general standard or specification that the endpoint implements." ;
    schema:description "A description of the service end-point, including its operations, parameters etc." ;
    schema:name "description of service end-point" ;
.

dcat:keyword
    a owl:DatatypeProperty ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf dct:subject ;
    schema:description "A keyword or tag describing a resource." ;
    schema:name "keyword" ;
.

dcat:landingPage
    a owl:ObjectProperty ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range foaf:Document ;
    rdfs:subPropertyOf foaf:page ;
    skos:scopeNote "If the distribution(s) are accessible only through a landing page (i.e. direct download URLs are not known), then the landing page link should be duplicated as accessURL on a distribution." ;
    schema:description "A Web page that can be navigated to in a Web browser to gain access to the catalog, a dataset, its distributions and/or additional information." ;
    schema:name "landing page" ;
.

dcat:mediaType
    a owl:ObjectProperty ;
    rdfs:domain dcat:Distribution ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range dct:MediaType ;
    rdfs:subPropertyOf dct:format ;
    skos:changeNote "The range of dcat:mediaType has been tightened as part of the revision of DCAT." ;
    skos:editorialNote "Status: English Definition text modified by DCAT revision team, Italian and Czech translation provided, other translations pending. Note some inconsistency on def vs. usage." ;
    skos:scopeNote "This property SHOULD be used when the media type of the distribution is defined in the IANA media types registry https://www.iana.org/assignments/media-types/, otherwise dct:format MAY be used with different values." ;
    schema:description "The media type of the distribution as defined by IANA." ;
    schema:name "media type" ;
.

dcat:packageFormat
    a owl:ObjectProperty ;
    rdfs:domain dcat:Distribution ;
    rdfs:isDefinedBy <https://www.w3.org/TR/vocab-dcat-2/> ;
    rdfs:range dct:MediaType ;
    rdfs:subPropertyOf dct:format ;
    skos:changeNote "New property added in DCAT 2.0." ;
    skos:scopeNote "This property to be used when the files in the distribution are packaged, e.g. in a TAR file, a Frictionless Data Package or a Bagit file. The format SHOULD be expressed using a media type as defined by IANA media types registry https://www.iana.org/assignments/media-types/, if available." ;
    schema:description "The package format of the distribution in which one or more data files are grouped together, e.g. to enable a set of related files to be downloaded together." ;
    schema:name "packaging format" ;
.

dcat:qualifiedRelation
    a owl:ObjectProperty ;
    rdfs:domain dcat:Resource ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range dcat:Relationship ;
    skos:changeNote "New property added in DCAT 2.0." ;
    skos:editorialNote "Introduced into DCAT to complement the other PROV qualified relations. " ;
    skos:scopeNote "Used to link to another resource where the nature of the relationship is known but does not match one of the standard Dublin Core properties (dct:hasPart, dct:isPartOf, dct:conformsTo, dct:isFormatOf, dct:hasFormat, dct:isVersionOf, dct:hasVersion, dct:replaces, dct:isReplacedBy, dct:references, dct:isReferencedBy, dct:requires, dct:isRequiredBy) or PROV-O properties (prov:wasDerivedFrom, prov:wasInfluencedBy, prov:wasQuotedFrom, prov:wasRevisionOf, prov:hadPrimarySource, prov:alternateOf, prov:specializationOf)." ;
    schema:description "Link to a description of a relationship with another resource." ;
    schema:name "qualified relation" ;
.

dcat:record
    a owl:ObjectProperty ;
    rdfs:domain dcat:Catalog ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range dcat:CatalogRecord ;
    skos:altLabel "har post"@da ;
    skos:editorialNote "Status: English, Italian, Spanish and Czech Definitions modified by DCAT revision team, other translations pending." ;
    schema:description "A record describing the registration of a single dataset or data service that is part of the catalog." ;
    schema:name "record" ;
.

dcat:servesDataset
    a owl:ObjectProperty ;
    rdfs:domain dcat:DataService ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range dcat:Dataset ;
    skos:altLabel "udstiller"@da ;
    skos:changeNote "New property in DCAT 2.0." ;
    schema:description "A collection of data that this DataService can distribute." ;
    schema:name "serves dataset" ;
.

dcat:service
    a owl:ObjectProperty ;
    rdfs:domain dcat:Catalog ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range dcat:DataService ;
    rdfs:subPropertyOf
        dct:hasPart ,
        rdfs:member ;
    skos:altLabel "har datatjeneste"@da ;
    skos:changeNote "New property added in DCAT 2.0." ;
    schema:description "A site or endpoint that is listed in the catalog." ;
    schema:name "service" ;
.

dcat:spatialResolutionInMeters
    a owl:DatatypeProperty ;
    rdfs:comment "minimum spatial separation resolvable in a dataset, measured in metres." ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range xsd:decimal ;
    skos:changeNote "New property added in DCAT 2.0." ;
    skos:editorialNote "Might appear in the description of a Dataset or a Distribution, so no domain is specified." ;
    skos:scopeNote """Alternative spatial resolutions might be provided as different dataset distributions.

If the dataset is an image or grid this should correspond to the spacing of items. For other kinds of spatial dataset, this property will usually indicate the smallest distance between items in the dataset.""" ;
    schema:description "minimum spatial separation resolvable in a dataset, measured in metres." ;
    schema:name "spatial resolution (metres)" ;
.

dcat:startDate
    a owl:DatatypeProperty ;
    rdfs:domain dct:PeriodOfTime ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range rdfs:Literal ;
    skos:changeNote "New property added in DCAT 2.0." ;
    skos:scopeNote "The range of this property is intentionally generic, with the purpose of allowing different level of temporal precision for specifying the start of a period. E.g., it can be expressed with a date (xsd:date), a date and time (xsd:dateTime), or a year (xsd:gYear)." ;
    schema:description "The start of the period" ;
    schema:name "start date" ;
.

dcat:temporalResolution
    a owl:DatatypeProperty ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range xsd:duration ;
    skos:changeNote "New property added in DCAT 2.0." ;
    skos:editorialNote "Might appear in the description of a Dataset or a Distribution, so no domain is specified." ;
    skos:scopeNote
        "Alternative temporal resolutions might be provided as different dataset distributions." ,
        "If the dataset is a time-series this should correspond to the spacing of items in the series. For other kinds of dataset, this property will usually indicate the smallest time difference between items in the dataset." ;
    schema:description "minimum time period resolvable in a dataset." ;
    schema:name "temporal resolution" ;
.

dcat:theme
    a owl:ObjectProperty ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range skos:Concept ;
    rdfs:subPropertyOf dct:subject ;
    skos:altLabel "tema"@da ;
    skos:editorialNote "Status: English Definition text modified by DCAT revision team, all except for Italian and Czech translations are pending." ;
    skos:scopeNote "The set of skos:Concepts used to categorize the resources are organized in a skos:ConceptScheme describing all the categories and their relations in the catalog." ;
    schema:description "A main category of the resource. A resource can have multiple themes." ;
    schema:name "theme" ;
.

dcat:themeTaxonomy
    a owl:ObjectProperty ;
    sdo:rangeIncludes
        owl:Ontology ,
        skos:Collection ,
        skos:ConceptScheme ;
    rdfs:domain dcat:Catalog ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range rdfs:Resource ;
    skos:altLabel "temataksonomi"@da ;
    skos:scopeNote "It is recommended that the taxonomy is organized in a skos:ConceptScheme, skos:Collection, owl:Ontology or similar, which allows each member to be denoted by an IRI and published as linked-data." ;
    schema:description "The knowledge organization system (KOS) used to classify catalog's datasets." ;
    schema:name "theme taxonomy" ;
.

<http://example.com/person/John-Erickson>
    a schema:Person ;
    schema:email ""^^xsd:anyURI ;
    schema:name "John Erickson" ;
.

<http://fadmaa.me/foaf.ttl>
    a schema:Person ;
    schema:email ""^^xsd:anyURI ;
    schema:name "Fadi Maali" ;
.

dcat:CatalogRecord
    a owl:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:allValuesFrom dcat:Resource ;
            owl:onProperty foaf:primaryTopic ;
        ] ,
        [
            a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty foaf:primaryTopic ;
        ] ;
    skos:editorialNote "English definition updated in this revision. Multilingual text not yet updated except the Spanish one and the Czech one and Italian one." ;
    skos:scopeNote "This class is optional and not all catalogs will use it. It exists for catalogs where a distinction is made between metadata about a dataset or data service and metadata about the entry for the dataset or data service in the catalog. For example, the publication date property of the dataset reflects the date when the information was originally made available by the publishing agency, while the publication date of the catalog record is the date when the dataset was added to the catalog. In cases where both dates differ, or where only the latter is known, the publication date should only be specified for the catalog record. Notice that the W3C PROV Ontology allows describing further provenance information such as the details of the process and the agent involved in a particular change to a dataset." ;
    schema:description "A record in a data catalog, describing the registration of a single dataset or data service." ;
    schema:name "Catalog Record" ;
.

dcat:Role
    a owl:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:seeAlso dcat:hadRole ;
    rdfs:subClassOf skos:Concept ;
    skos:changeNote "New class added in DCAT 2.0." ;
    skos:editorialNote "Introduced into DCAT to complement prov:Role (whose use is limited to roles in the context of an activity, as the range of prov:hadRole)." ;
    skos:scopeNote
        "Used in a qualified-attribution to specify the role of an Agent with respect to an Entity. It is recommended that the values be managed as a controlled vocabulary of agent roles, such as http://registry.it.csiro.au/def/isotc211/CI_RoleCode." ,
        "Used in a qualified-relation to specify the role of an Entity with respect to another Entity. It is recommended that the values be managed as a controlled vocabulary of entity roles such as: ISO 19115 DS_AssociationTypeCode http://registry.it.csiro.au/def/isotc211/DS_AssociationTypeCode; IANA Registry of Link Relations https://www.iana.org/assignments/link-relation;  DataCite metadata schema;  MARC relators https://id.loc.gov/vocabulary/relators." ;
    schema:description "A role is the function of a resource or agent with respect to another resource, in the context of resource attribution or resource relationships." ;
    schema:name "Role" ;
.

dcat:accessService
    a owl:ObjectProperty ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range dcat:DataService ;
    skos:changeNote "New property added in DCAT 2.0." ;
    schema:description "A site or end-point that gives access to the distribution of the dataset." ;
    schema:name "data access service" ;
.

dcat:endpointURL
    a owl:ObjectProperty ;
    rdfs:domain dcat:DataService ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range rdfs:Resource ;
    skos:changeNote "New property in DCAT 2.0." ;
    schema:description "The root location or primary endpoint of the service (a web-resolvable IRI)." ;
    schema:name "service end-point" ;
.

dcat:hadRole
    a owl:ObjectProperty ;
    rdfs:domain
        [
            owl:unionOf (
                prov:Attribution
                dcat:Relationship
            ) ;
        ] ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:range dcat:Role ;
    skos:changeNote "New property added in DCAT 2.0." ;
    skos:editorialNote "Introduced into DCAT to complement prov:hadRole (whose use is limited to roles in the context of an activity, with the domain of prov:Association." ;
    skos:scopeNote
        "May be used in a qualified-attribution to specify the role of an Agent with respect to an Entity. It is recommended that the value be taken from a controlled vocabulary of agent roles, such as http://registry.it.csiro.au/def/isotc211/CI_RoleCode." ,
        "May be used in a qualified-relation to specify the role of an Entity with respect to another Entity.  It is recommended that the value be taken from a controlled vocabulary of entity roles such as: ISO 19115 DS_AssociationTypeCode http://registry.it.csiro.au/def/isotc211/DS_AssociationTypeCode; IANA Registry of Link Relations https://www.iana.org/assignments/link-relation; DataCite metadata schema; MARC relators https://id.loc.gov/vocabulary/relators." ;
    schema:description "The function of an entity or agent with respect to another entity or resource." ;
    schema:name "hadRole" ;
.

dcat:Relationship
    a owl:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    skos:changeNote "New class added in DCAT 2.0." ;
    skos:scopeNote "Use to characterize a relationship between datasets, and potentially other resources, where the nature of the relationship is known but is not adequately characterized by the standard Dublin Core properties (dct:hasPart, dct:isPartOf, dct:conformsTo, dct:isFormatOf, dct:hasFormat, dct:isVersionOf, dct:hasVersion, dct:replaces, dct:isReplacedBy, dct:references, dct:isReferencedBy, dct:requires, dct:isRequiredBy) or PROV-O properties (prov:wasDerivedFrom, prov:wasInfluencedBy, prov:wasQuotedFrom, prov:wasRevisionOf, prov:hadPrimarySource, prov:alternateOf, prov:specializationOf)." ;
    schema:description "An association class for attaching additional information to a relationship between DCAT Resources." ;
    schema:name "Relationship" ;
.

dcat:Dataset
    a owl:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:subClassOf dcat:Resource ;
    skos:altLabel "Datasamling"@da ;
    skos:changeNote "2018-02 - subclass of dctype:Dataset removed because scope of dcat:Dataset includes several other types from the dctype vocabulary." ;
    skos:editorialNote "2020-03-16 A new scopenote added and need to be translated" ;
    skos:scopeNote
        "The notion of dataset in DCAT is broad and inclusive, with the intention of accommodating resource types arising from all communities. Data comes in many forms including numbers, text, pixels, imagery, sound and other multi-media, and potentially other types, any of which might be collected into a dataset." ,
        "This class describes the conceptual dataset. One or more representations might be available, with differing schematic layouts and formats or serializations." ,
        "This class represents the actual dataset as published by the dataset provider. In cases where a distinction between the actual dataset and its entry in the catalog is necessary (because metadata such as modification date and maintainer might differ), the catalog record class can be used for the latter." ;
    schema:description "A collection of data, published or curated by a single source, and available for access or download in one or more represenations." ;
    schema:name "Dataset" ;
.

dcat:Resource
    a owl:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    skos:changeNote "New class added in DCAT 2.0." ;
    skos:scopeNote
        "The class of all catalogued resources, the Superclass of dcat:Dataset, dcat:DataService, dcat:Catalog and any other member of a dcat:Catalog. This class carries properties common to all catalogued resources, including datasets and data services. It is strongly recommended to use a more specific sub-class. When describing a resource which is not a dcat:Dataset or dcat:DataService, it is recommended to create a suitable sub-class of dcat:Resource, or use dcat:Resource with the dct:type property to indicate the specific type." ,
        "dcat:Resource is an extension point that enables the definition of any kind of catalog. Additional subclasses may be defined in a DCAT profile or application for catalogs of other kinds of resources." ;
    schema:description "Resource published or curated by a single agent." ;
    schema:name "Catalogued resource" ;
.

dcat:DataService
    a owl:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:subClassOf
        dctype:Service ,
        dcat:Resource ;
    skos:altLabel "Dataservice"@da ;
    skos:changeNote "New class added in DCAT 2.0." ;
    skos:scopeNote
        "If a dcat:DataService is bound to one or more specified Datasets, they are indicated by the dcat:servesDataset property." ,
        "The kind of service can be indicated using the dct:type property. Its value may be taken from a controlled vocabulary such as the INSPIRE spatial data service type vocabulary." ;
    schema:description "A site or end-point providing operations related to the discovery of, access to, or processing functions on, data or related resources." ;
    schema:name "Data service" ;
.

dcat:Catalog
    a owl:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    rdfs:subClassOf dcat:Dataset ;
    skos:editorialNote "English, Italian, Spanish definitions updated in this revision. Multilingual text not yet updated." ;
    skos:scopeNote "A web-based data catalog is typically represented as a single instance of this class." ;
    schema:description "A curated collection of metadata about resources (e.g., datasets and data services in the context of a data catalog)." ;
    schema:name "Catalog" ;
.

dcat:Distribution
    a owl:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/dcat> ;
    skos:altLabel "Dataudstilling"@da ;
    skos:scopeNote "This represents a general availability of a dataset it implies no information about the actual access method of the data, i.e. whether by direct download, API, or through a Web page. The use of dcat:downloadURL property indicates directly downloadable distributions." ;
    schema:description "A specific representation of a dataset. A dataset might be available in multiple serializations that may differ in various ways, including natural language, media-type or format, schematic organization, temporal and spatial resolution, level of detail or profiles (which might specify any or all of the above)." ;
    schema:name "Distribution" ;
.

