PREFIX bfo: <http://www.ifomis.org/bfo/1.1#>
PREFIX ont: <http://www.ifomis.org/bfo/1.1>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX snap: <http://www.ifomis.org/bfo/1.1/snap#>
PREFIX span: <http://www.ifomis.org/bfo/1.1/span#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

bfo:Entity
    a owl:Class ;
    rdfs:label "entity" ;
    rdfs:isDefinedBy ont: ;
    owl:unionOf (
        snap:Continuant
        span:Occurrent
    ) ;
    schema:description "An entity - thing" ;
    skos:prefLabel "entity" ;
.

snap:Disposition
    a owl:Class ;
    rdfs:label "disposition" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:RealizableEntity ;
    owl:disjointWith
        snap:Function ,
        snap:Role ;
    skos:definition "A realizable entity that essentially causes a specific process or transformation in the object  in which it inheres, under specific circumstances and in conjunction with the laws of nature. A general formula for dispositions is: X (object  has the disposition D to (transform, initiate a process) R under conditions C."@en ;
    skos:example
        "the disposition of a patient with a weakened immune system to contract disease" ,
        "the disposition of a vase to brake if dropped" ,
        "the disposition of blood to coagulate" ,
        "the disposition of metal to conduct electricity" ,
        "the disposition of vegetables to decay when not refrigerated" ;
    skos:prefLabel "disposition" ;
.

snap:Function
    a owl:Class ;
    rdfs:label "function" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:RealizableEntity ;
    owl:disjointWith
        snap:Disposition ,
        snap:Role ;
    skos:definition "A realizable entity  the manifestation of which is an essentially end-directed activity of a continuant  entity in virtue of that continuant  entity being a specific kind of entity in the kind or kinds of contexts that it is made for."@en ;
    skos:example
        "the digestive function of the stomach to nutriate the body" ,
        "the function of a birth canal to enable transport" ,
        "the function of a computer program to compute mathematical equations" ,
        "the function of a hammer to drive in nails" ,
        "the function of a judge in a court of law" ,
        "the function of an automobile to provide transportation" ,
        "the function of reproduction in the transmission of genetic material" ,
        "the function of the heart in the body: to pump blood, to receive de-oxygenated and oxygenated blood, etc." ;
    skos:prefLabel "function" ;
.

snap:GenericallyDependentContinuant
    a owl:Class ;
    rdfs:label "generically_dependent_continuant" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:DependentContinuant ;
    owl:disjointWith snap:SpecificallyDependentContinuant ;
    skos:definition "A continuant  that is dependent on one or other independent continuant  bearers. For every instance of A requires some instance of (an independent continuant  type) B but which instance of B serves can change from time to time."@en ;
    skos:example "a certain PDF file that exists in different and in several hard drives" ;
    skos:prefLabel "generically_dependent_continuant" ;
.

snap:Quality
    a owl:Class ;
    rdfs:label "quality" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:SpecificallyDependentContinuant ;
    owl:disjointWith snap:RealizableEntity ;
    skos:definition "A specifically dependent continuant  that is exhibited if it inheres in an entity or entities at all (a categorical property)."@en ;
    skos:example
        "the ambient temperature of air" ,
        "the circumference of a waist" ,
        "the color of a tomato" ,
        "the mass of a piece of gold" ,
        "the shape of a nose" ,
        "the weight of a chimpanzee" ;
    skos:prefLabel "quality" ;
.

snap:Role
    a owl:Class ;
    rdfs:label "role" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:RealizableEntity ;
    owl:disjointWith
        snap:Disposition ,
        snap:Function ;
    skos:definition "A realizable entity  the manifestation of which brings about some result or end that is not essential to a continuant  in virtue of the kind of thing that it is but that can be served or participated in by that kind of continuant  in some kinds of natural, social or institutional contexts."@en ;
    skos:example
        "the role of a biological grandfather as legal guardian in the context of asystem of laws" ,
        "the role of a chemical compound in an experiment" ,
        "the role of a patient relative as defined by a hospital administrative form" ,
        "the role of a person as a surgeon" ,
        "the role of a student in a university" ,
        "the role of a woman as a legal mother in the context of system of laws" ,
        "the role of ingested matter in digestion" ;
    skos:prefLabel "role" ;
.

span:ScatteredSpatiotemporalRegion
    a owl:Class ;
    rdfs:label "scattered_spatiotemporal_region" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:SpatiotemporalRegion ;
    owl:disjointWith span:ConnectedSpatiotemporalRegion ;
    skos:definition "A space time region  that has spatial and temporal dimensions and every spatial and temporal point of which is not connected with every other spatial and temporal point of which."@en ;
    skos:example
        "the space and time occupied by the individual games of the World Cup" ,
        "the space and time occupied by the individual liaisons in a romantic affair" ;
    skos:prefLabel "scattered_spatiotemporal_region" ;
.

span:ScatteredTemporalRegion
    a owl:Class ;
    rdfs:label "scattered_temporal_region" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:TemporalRegion ;
    owl:disjointWith span:ConnectedTemporalRegion ;
    skos:definition "A temporal region  every point of which is not mediately or immediately connected with every other point of which."@en ;
    skos:example
        "the time occupied by the individual games of the World Cup" ,
        "the time occupied by the individual liaisons in a romantic affair" ;
    skos:prefLabel "scattered_temporal_region" ;
.

span:SpatiotemporalInstant
    a owl:Class ;
    rdfs:label "spatiotemporal_instant" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:ConnectedSpatiotemporalRegion ;
    owl:disjointWith span:SpatiotemporalInterval ;
    skos:definition "A connected space time region  at a specific moment."@en ;
    skos:example "the space time region occupied by a single instantaneous temporalslice (part) of a process" ;
    skos:prefLabel "spatiotemporal_instant" ;
.

span:SpatiotemporalInterval
    a owl:Class ;
    rdfs:label "spatiotemporal_interval" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:ConnectedSpatiotemporalRegion ;
    owl:disjointWith span:SpatiotemporalInstant ;
    skos:definition "A connected space time region  that endures for more than a single moment of time."@en ;
    skos:example "the space time region occupied by a process or by a fiat processual part" ;
    skos:prefLabel "spatiotemporal_interval" ;
.

span:TemporalInstant
    a owl:Class ;
    rdfs:label "temporal_instant" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:ConnectedTemporalRegion ;
    owl:disjointWith span:TemporalInterval ;
    skos:definition "A connected temporal region  comprising a single moment of time."@en ;
    skos:example
        "right now" ,
        "the moment at which a child is born" ,
        "the moment at which a finger is detached in an industrial accident" ,
        "the moment of death" ;
    skos:prefLabel "temporal_instant" ;
.

span:TemporalInterval
    a owl:Class ;
    rdfs:label "temporal_interval" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:ConnectedTemporalRegion ;
    owl:disjointWith span:TemporalInstant ;
    skos:definition "A connected temporal region  lasting for more than a single moment of time."@en ;
    skos:example "any continuous temporal duration during which a process occurs" ;
    skos:prefLabel "temporal_interval" ;
.

snap:OneDimensionalRegion
    a owl:Class ;
    rdfs:label "one_dimensional_region" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:SpatialRegion ;
    owl:disjointWith
        snap:ThreeDimensionalRegion ,
        snap:TwoDimensionalRegion ,
        snap:ZeroDimensionalRegion ;
    skos:definition "A spatial region  with one dimension."@en ;
    skos:example
        "an edge of a cube-shaped part of space" ,
        "the part of space that is a line stretching from one end of absolute spaceto the other" ;
    skos:prefLabel "one_dimensional_region" ;
.

snap:SpecificallyDependentContinuant
    a owl:Class ;
    rdfs:label "specifically_dependent_continuant" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:DependentContinuant ;
    owl:disjointWith snap:GenericallyDependentContinuant ;
    owl:equivalentClass
        [
            a owl:Class ;
            rdfs:isDefinedBy ont: ;
            owl:unionOf (
                snap:Quality
                snap:RealizableEntity
            ) ;
            schema:description "Union Class" ;
            schema:name "Union Class" ;
        ] ;
    skos:altLabel
        "mode" ,
        "property" ,
        "trope" ;
    skos:definition "A continuant  that inheres in or is borne by other entities. Every instance of A requires some specific instance of B which must always be the same."@en ;
    skos:example
        "the color of a tomato" ,
        "the disposition of fish to decay" ,
        "the function of the heart in the body: to pump blood, to receive de-oxygenated and oxygenated blood, etc." ,
        "the liquidity of blood" ,
        "the mass of a cloud" ,
        "the role of being a doctor" ,
        "the smell of mozzarella" ;
    skos:prefLabel "specifically_dependent_continuant" ;
.

snap:ThreeDimensionalRegion
    a owl:Class ;
    rdfs:label "three_dimensional_region" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:SpatialRegion ;
    owl:disjointWith
        snap:OneDimensionalRegion ,
        snap:TwoDimensionalRegion ,
        snap:ZeroDimensionalRegion ;
    skos:definition "A spatial region  with three dimensions."@en ;
    skos:example
        "a cube-shaped part of space" ,
        "a sphere-shaped part of space" ;
    skos:prefLabel "three_dimensional_region" ;
.

snap:TwoDimensionalRegion
    a owl:Class ;
    rdfs:label "two_dimensional_region" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:SpatialRegion ;
    owl:disjointWith
        snap:OneDimensionalRegion ,
        snap:ThreeDimensionalRegion ,
        snap:ZeroDimensionalRegion ;
    skos:definition "A spatial region  with two dimensions."@en ;
    skos:example
        "the surface of a cube-shaped part of space" ,
        "the surface of a rectilinear planar figure-shaped part of space" ,
        "the surface of a sphere-shaped part of space" ;
    skos:prefLabel "two_dimensional_region" ;
.

snap:ZeroDimensionalRegion
    a owl:Class ;
    rdfs:label "zero_dimensional_region" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:SpatialRegion ;
    owl:disjointWith
        snap:OneDimensionalRegion ,
        snap:ThreeDimensionalRegion ,
        snap:TwoDimensionalRegion ;
    skos:definition "A spatial region  with no dimensions."@en ;
    skos:example "a point" ;
    skos:prefLabel "zero_dimensional_region" ;
.

span:ConnectedSpatiotemporalRegion
    a owl:Class ;
    rdfs:label "connected_spatiotemporal_region" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:SpatiotemporalRegion ;
    owl:disjointWith span:ScatteredSpatiotemporalRegion ;
    owl:equivalentClass
        [
            a owl:Class ;
            rdfs:isDefinedBy ont: ;
            owl:unionOf (
                span:SpatiotemporalInstant
                span:SpatiotemporalInterval
            ) ;
            schema:name "Union of SpatiotemporalInstant & SpatiotemporalInterval" ;
            schema:description "Union Class" ;
        ] ;
    skos:definition "A space time region  that has temporal and spatial dimensions such that all points within the spatiotemporal region are mediately or immediately connected to all other points within the same space time region ."@en ;
    skos:example
        "the spatial and temporal location of an individual organism's life" ,
        "the spatial and temporal location of the development of a fetus" ;
    skos:prefLabel "connected_spatiotemporal_region" ;
.

span:ConnectedTemporalRegion
    a owl:Class ;
    rdfs:label "connected_temporal_region" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:TemporalRegion ;
    owl:disjointWith span:ScatteredTemporalRegion ;
    owl:equivalentClass
        [
            a owl:Class ;
            rdfs:isDefinedBy ont: ;
            owl:unionOf (
                span:TemporalInstant
                span:TemporalInterval
            ) ;
            schema:description "Union Class" ;
            schema:name "Union Class" ;
        ] ;
    skos:definition "A temporal region  every point of which is mediately or immediately connected with every other point of which."@en ;
    skos:example
        "the 1970s years" ,
        "the time from the beginning to the end of a heart attack" ,
        "the time taken up by cellular meiosis" ;
    skos:prefLabel "connected_temporal_region" ;
.

snap:Continuant
    a owl:Class ;
    rdfs:label "continuant" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf bfo:Entity ;
    owl:disjointWith span:Occurrent ;
    owl:equivalentClass
        [
            a owl:Class ;
            rdfs:isDefinedBy ont: ;
            owl:unionOf (
                snap:DependentContinuant
                snap:IndependentContinuant
                snap:SpatialRegion
            ) ;
            schema:description "Union Class" ;
            schema:name "Union Class" ;
        ] ;
    skos:altLabel "endurant" ;
    skos:definition "An entity  that exists in full at any time in which it exists at all, persists through time while maintaining its identity and has no temporal parts."@en ;
    skos:example
        "a heart" ,
        "a person" ,
        "a symphony orchestra" ,
        "the color of a tomato" ,
        "the disposition of blood to coagulate" ,
        "the lawn and atmosphere in front of our building" ,
        "the mass of a cloud" ;
    skos:prefLabel "continuant" ;
.

snap:DependentContinuant
    a owl:Class ;
    rdfs:label "dependent_continuant" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:Continuant ;
    owl:disjointWith
        snap:IndependentContinuant ,
        snap:SpatialRegion ;
    owl:equivalentClass
        [
            a owl:Class ;
            rdfs:isDefinedBy ont: ;
            owl:unionOf (
                snap:GenericallyDependentContinuant
                snap:SpecificallyDependentContinuant
            ) ;
            schema:description "Union Class" ;
            schema:name "Union Class" ;
        ] ;
    skos:definition "A continuant  that is either dependent on one or other independent continuant  bearers or inheres in or is borne by other entities."@en ;
    skos:prefLabel "dependent_continuant" ;
.

snap:FiatObjectPart
    a owl:Class ;
    rdfs:label "fiat_object_part" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:IndependentContinuant ;
    owl:disjointWith
        snap:Object ,
        snap:ObjectAggregate ,
        snap:ObjectBoundary ,
        snap:Site ;
    skos:altLabel "fiat_substance_part" ;
    skos:definition "An independent continuant  that is part of an object  but is not demarcated by any physical discontinuities."@en ;
    skos:example
        "the dorsal and ventral surfaces of the body" ,
        "the east side of Saarbruecken" ,
        "the lower right portion of a human torso" ,
        "the upper and lower lobes of the left lung" ;
    skos:prefLabel "fiat_object_part" ;
.

snap:Object
    a owl:Class ;
    rdfs:label "object" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:IndependentContinuant ;
    owl:disjointWith
        snap:FiatObjectPart ,
        snap:ObjectAggregate ,
        snap:ObjectBoundary ,
        snap:Site ;
    skos:altLabel "substance" ;
    skos:definition "An independent continuant  that is spatially extended, maximally self-connected and self-contained (the parts of a substance are not separated from each other by spatial gaps) and possesses an internal unity. The identity of substantial object  entities is independent of that of other entities and can be maintained through time."@en ;
    skos:example
        "a chair" ,
        "a heart" ,
        "a lung" ,
        "an apple" ,
        "an organism" ;
    skos:prefLabel "object" ;
.

snap:ObjectAggregate
    a owl:Class ;
    rdfs:label "object_aggregate" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:IndependentContinuant ;
    owl:disjointWith
        snap:FiatObjectPart ,
        snap:Object ,
        snap:ObjectBoundary ,
        snap:Site ;
    skos:altLabel "substance_aggregate" ;
    skos:definition "An independent continuant  that is a mereological sum of separate object  entities and possesses non-connected boundaries."@en ;
    skos:example
        "a collection of random bacteria" ,
        "a flock of geese" ,
        "a group of commuters on the subway" ,
        "a heap of stones" ,
        "the patients in a hospital" ;
    skos:prefLabel "object_aggregate" ;
.

snap:ObjectBoundary
    a owl:Class ;
    rdfs:label "object_boundary" ;
    rdfs:comment "Boundaries are theoretically difficult entities to account for, however the intuitive notion of a physical boundary as a surface of some sort (whether inside or outside of a thing) will generally serve as a good guide for the use of this universal." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:IndependentContinuant ;
    owl:disjointWith
        snap:FiatObjectPart ,
        snap:Object ,
        snap:ObjectAggregate ,
        snap:Site ;
    skos:altLabel "substance_boundary" ;
    skos:definition "An independent continuant  that is a lower dimensional part of a spatial entity, normally a closed two-dimensional surface. Boundaries are those privileged parts of object  entities that exist at exactly the point where the object  is separated off from the rest of the existing entities in the world."@en ;
    skos:example
        "the outer surface of a cell or cell wall" ,
        "the surface of the earth" ,
        "the surface of the interior of the stomach" ,
        "the surface of the skin" ;
    skos:prefLabel "object_boundary" ;
.

snap:RealizableEntity
    a owl:Class ;
    rdfs:label "realizable_entity" ;
    rdfs:comment "If a realizable entity  inheres in a continuant , this does not imply that it is actually realized." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:SpecificallyDependentContinuant ;
    owl:disjointWith snap:Quality ;
    skos:definition "A specifically dependent continuant  that inheres in continuant  entities and are not exhibited in full at every time in which it inheres in an entity or group of entities. The exhibition or actualization of a realizable entity is a particular manifestation, functioning or process that occurs under certain circumstances."@en ;
    skos:example
        "the disposition of blood to coagulate" ,
        "the disposition of metal to conduct electricity" ,
        "the function of the reproductive organs" ,
        "the role of being a doctor" ;
    skos:prefLabel "realizable_entity" ;
.

snap:Site
    a owl:Class ;
    rdfs:label "site" ;
    rdfs:comment "An instance of Site  is a mixture of independent continuant entities which act as surrounding environments for other independent continuant entities, most importantly for instances of object . A site  is typically made of object  or fiat object part  entities and a surrounding medium in which is found an object  occupying the site . Independent continuant  entities may be associated with others (which, then, are sites) through a relation of \"occupation\". That relation is connected to, but distinct from, the relation of spatial location. Site entities are not to be confused with spatial region  entities. In BFO, site  allows for a so-called relational view of space which is different from the view corresponding to the class spatial region  (see the comment on this class)." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:IndependentContinuant ;
    owl:disjointWith
        snap:FiatObjectPart ,
        snap:Object ,
        snap:ObjectAggregate ,
        snap:ObjectBoundary ;
    skos:definition "An independent continuant  consisting of a characteristic spatial shape in relation to some arrangement of other continuant  entities and of the medium which is enclosed in whole or in part by this characteristic spatial shape. Site  entities are entities that can be occupied by other continuant entities."@en ;
    skos:example
        "Maria's nostril or her intestines for a variety of bacteria" ,
        "a particular room in a particular hospital" ;
    skos:prefLabel "site" ;
.

span:FiatProcessPart
    a owl:Class ;
    rdfs:label "fiat_process_part" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:ProcessualEntity ;
    owl:disjointWith
        span:Process ,
        span:ProcessAggregate ,
        span:ProcessBoundary ,
        span:ProcessualContext ;
    skos:definition "A processual entity  that is part of a process but that does not have bona fide beginnings and endings corresponding to real discontinuities."@en ;
    skos:example
        "chewing during a meal" ,
        "the middle part of a rainstorm" ,
        "the most interesting part of Van Gogh's life" ,
        "the worst part of a heart-attack" ;
    skos:prefLabel "fiat_process_part" ;
.

span:Occurrent
    a owl:Class ;
    rdfs:label "occurrent" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf bfo:Entity ;
    owl:disjointWith snap:Continuant ;
    owl:equivalentClass
        [
            a owl:Class ;
            rdfs:isDefinedBy ont: ;
            owl:unionOf (
                span:ProcessualEntity
                span:SpatiotemporalRegion
                span:TemporalRegion
            ) ;
            schema:description "Union Class" ;
            schema:name "Union Class" ;
        ] ;
    skos:altLabel "perdurant" ;
    skos:definition "An entity  that has temporal parts and that happens, unfolds or develops through time. Sometimes also called perdurants."@en ;
    skos:example
        "a surgical operation as processual context for a nosocomical infection" ,
        "the life of an organism" ,
        "the most interesting part of Van Gogh's life" ,
        "the spatiotemporal context occupied by a process of cellular meiosis" ,
        "the spatiotemporal region occupied by the development of a cancer tumor" ;
    skos:prefLabel "occurrent" ;
.

span:Process
    a owl:Class ;
    rdfs:label "process" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:ProcessualEntity ;
    owl:disjointWith
        span:FiatProcessPart ,
        span:ProcessAggregate ,
        span:ProcessBoundary ,
        span:ProcessualContext ;
    skos:definition "A processual entity  that is a maximally connected spatiotemporal whole and has bona fide beginnings and endings corresponding to real discontinuities."@en ;
    skos:example
        "the life of an organism" ,
        "the process of cell-division" ,
        "the process of sleeping" ;
    skos:prefLabel "process" ;
.

span:ProcessAggregate
    a owl:Class ;
    rdfs:label "process_aggregate" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:ProcessualEntity ;
    owl:disjointWith
        span:FiatProcessPart ,
        span:Process ,
        span:ProcessBoundary ,
        span:ProcessualContext ;
    skos:definition "A processual entity  that is a mereological sum of process entities and possesses non-connected boundaries."@en ;
    skos:example
        "a process of digestion and a process of thinking taken together" ,
        "the beating of the hearts of each of seven individuals in the room" ,
        "the playing of each of the members of an orchestra" ;
    skos:prefLabel "process_aggregate" ;
.

span:ProcessBoundary
    a owl:Class ;
    rdfs:label "process_boundary" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:ProcessualEntity ;
    owl:disjointWith
        span:FiatProcessPart ,
        span:Process ,
        span:ProcessAggregate ,
        span:ProcessualContext ;
    skos:definition "A processual entity  that is the fiat or bona fide instantaneous temporal process boundary."@en ;
    skos:example
        "birth" ,
        "death" ,
        "the detaching of a finger in an industrial accident" ,
        "the final separation of two cells at the end of cell-division" ,
        "the forming of a synapse" ,
        "the incision at the beginning of a surgery" ,
        "the onset of REM sleep" ;
    skos:prefLabel "process_boundary" ;
.

span:ProcessualContext
    a owl:Class ;
    rdfs:label "processual_context" ;
    rdfs:comment
        "An instance of a processual context  is a mixture of processual entity which stand as surrounding environments for other processual entity  entities. The class processual context  is the analogous among occurrent  entities to the class site  among continuant  entities." ,
        "The processual context for a given manipulation occurring as part of an experiment is made of processual entities which occur in parallel, are not necessarily all parts of the experiment themselves and may involve continuant entities which are in the spatial vicinity of the participants in the experiment." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:ProcessualEntity ;
    owl:disjointWith
        span:FiatProcessPart ,
        span:Process ,
        span:ProcessAggregate ,
        span:ProcessBoundary ;
    skos:altLabel "setting" ;
    skos:definition "An occurrent  consisting of a characteristic spatial shape inhering in some arrangement of other occurrent  entities. processual context  entities are characteristically entities at or in which other occurrent  entities can be located or occur."@en ;
    skos:prefLabel "processual_context" ;
.

span:SpatiotemporalRegion
    a owl:Class ;
    rdfs:label "spatiotemporal_region" ;
    rdfs:comment
        "All instances of occurrent  are spatiotemporal entities, that is, they enter in the relation of (spatiotemporal) location with spatiotemporal region  entities. As a particular case, the exact spatiotemporal location of a spatiotemporal region  is this region itself." ,
        "An instance of the spatiotemporal region  is a part of spacetime. All parts of spacetime are spatiotemporal region  entities and only spatiotemporal region  entities are parts of spacetime. In particular, neither spatial region entities nor temporal region  entities are in BFO parts of spacetime. Spacetime is the entire extent of the spatiotemporal universe, a designated individual, which is thus itself a spatiotemporal region . Spacetime is among occurrents the analogous of space among continuant  entities." ,
        "Spacetime and spatiotemporal region  entities are entities in their own rights which exist independently of any entities which can be located at them. This view of spacetime can be called \"absolutist\" or \"the container view\". In BFO, the class processual context  allows for a so-called relational view of spacetime, that is to say, a view according to which spatiotemporality is a matter of relative location between entities and not a matter of being tied to spacetime. In BFO, the bridge between these two views is secured through the fact that instances of processual context  are too spatiotemporal entities." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:Occurrent ;
    owl:disjointWith
        span:ProcessualEntity ,
        span:TemporalRegion ;
    owl:equivalentClass
        [
            a owl:Class ;
            rdfs:isDefinedBy ont: ;
            owl:unionOf (
                span:ConnectedSpatiotemporalRegion
                span:ScatteredSpatiotemporalRegion
            ) ;
            schema:description "Union Class" ;
            schema:name "Union Class" ;
        ] ;
    skos:definition "An occurrent  at or in which processual entity  entities can be located."@en ;
    skos:example
        "the spatiotemporal context occupied by a process of cellular meiosis" ,
        "the spatiotemporal region occupied by a human life" ,
        "the spatiotemporal region occupied by the development of a cancer tumor" ;
    skos:prefLabel "spatiotemporal_region" ;
.

span:TemporalRegion
    a owl:Class ;
    rdfs:label "temporal_region" ;
    rdfs:comment
        "All instances of occurrent  are temporal entities, that is, they enter in the relation of (temporal) location with temporal region  entities. As a particular case, the exact spatiotemporal location of a temporal region  is this region itself. Continuant  entities are not temporal entities in the technical sense just explained; they are related to time in a different way, not through temporal location but through a relation of existence at a time or during a period of time (see continuant ." ,
        "An instance of temporal region  is a part of time. All parts of time are temporal region  entities and only temporal region  entities are parts of time. Time is the entire extent of the temporal universe, a designated individual, which is thus a temporal region itself." ,
        "Time and temporal region  entities are entities in their own rights which exist independently of any entities which can be located at them. This view of time can be called \"absolutist\" or \"the container view\" in analogy to what is traditionally the case with space (see spatial region ." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:Occurrent ;
    owl:disjointWith
        span:ProcessualEntity ,
        span:SpatiotemporalRegion ;
    owl:equivalentClass
        [
            a owl:Class ;
            rdfs:isDefinedBy ont: ;
            owl:unionOf (
                span:ConnectedTemporalRegion
                span:ScatteredTemporalRegion
            ) ;
            schema:description "Union Class" ;
            schema:name "Union Class" ;
        ] ;
    skos:definition "An occurrent  that is part of time."@en ;
    skos:example
        "the duration of a surgical procedure" ,
        "the moment of death" ,
        "the time it takes to run a marathon" ;
    skos:prefLabel "temporal_region" ;
.

snap:SpatialRegion
    a owl:Class ;
    rdfs:label "spatial_region" ;
    rdfs:comment
        "All instances of continuant  are spatial entities, that is, they enter in the relation of (spatial) location with spatial region  entities. As a particular case, the exact spatial location of a spatial region  is this region itself." ,
        "An instance of spatial region  is a part of space. All parts of space are spatial region  entities and only spatial region  entities are parts of space. Space is the entire extent of the spatial universe, a designated individual, which is thus itself a spatial region ." ,
        "Space and spatial region  entities are entities in their own rights which exist independently of any entities which can be located at them. This view of space is sometimes called \"absolutist\" or \"the container view\". In BFO, the class site  allows for a so-called relational view of space, that is to say, a view according to which spatiality is a matter of relative location between entities and not a matter of being tied to space. The bridge between these two views is secured through the fact that while instances of site  are not spatial region  entities, they are nevertheless spatial entities." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:Continuant ;
    owl:disjointWith
        snap:DependentContinuant ,
        snap:IndependentContinuant ;
    owl:equivalentClass
        [
            a owl:Class ;
            rdfs:isDefinedBy ont: ;
            owl:unionOf (
                snap:OneDimensionalRegion
                snap:ThreeDimensionalRegion
                snap:TwoDimensionalRegion
                snap:ZeroDimensionalRegion
            ) ;
            schema:description "Union Class" ;
            schema:name "Union Class" ;
        ] ;
    skos:definition "A continuant  that is neither bearer of quality  entities nor inheres in any other entities."@en ;
    skos:example
        "parts of the sum total of all space in the universe" ,
        "the sum total of all space in the universe" ;
    skos:prefLabel "spatial_region" ;
.

snap:IndependentContinuant
    a owl:Class ;
    rdfs:label "independent_continuant" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf snap:Continuant ;
    owl:disjointWith
        snap:DependentContinuant ,
        snap:SpatialRegion ;
    owl:equivalentClass
        [
            a owl:Class ;
            rdfs:isDefinedBy ont: ;
            owl:unionOf (
                snap:FiatObjectPart
                snap:Object
                snap:ObjectAggregate
                snap:ObjectBoundary
                snap:Site
            ) ;
            schema:description "Union Class" ;
            schema:name "Union Class" ;
        ] ;
    skos:altLabel "substantial_entity" ;
    skos:definition "A continuant  that is a bearer of quality  and realizable entity  entities, in which other entities inhere and which itself cannot inhere in anything."@en ;
    skos:example
        "a chair" ,
        "a heart" ,
        "a leg" ,
        "a person" ,
        "a symphony orchestra" ,
        "an organism" ,
        "the bottom right portion of a human torso" ,
        "the lawn and atmosphere in front of our building" ;
    skos:prefLabel "independent_continuant" ;
.

span:ProcessualEntity
    a owl:Class ;
    rdfs:label "processual_entity" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf span:Occurrent ;
    owl:disjointWith
        span:SpatiotemporalRegion ,
        span:TemporalRegion ;
    owl:equivalentClass
        [
            a owl:Class ;
            rdfs:isDefinedBy ont: ;
            owl:unionOf (
                span:FiatProcessPart
                span:Process
                span:ProcessAggregate
                span:ProcessBoundary
                span:ProcessualContext
            ) ;
            schema:description "Union Class" ;
            schema:name "Union Class" ;
        ] ;
    skos:definition "An occurrent  that exists in time by occurring or happening, has temporal parts and always involves and depends on some entity."@en ;
    skos:example
        "the course of a disease" ,
        "the flight of a bird" ,
        "the life of an organism" ,
        "the process of meiosis" ;
    skos:prefLabel "processual_entity" ;
.

