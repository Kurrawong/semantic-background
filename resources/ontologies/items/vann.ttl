    schema:dateCreated "2010-06-07"^^xsd:date ;
    schema:dateIssued "2010-06-07"^^xsd:date ;
    schema:dateModified "2010-06-07"^^xsd:date ;
    schema:description "This document describes a vocabulary for annotating descriptions of vocabularies with examples and usage notes." ;
    schema:identifier "http://purl.org/vocab/vann/vann-vocab-20050401" ;
    schema:license <http://creativecommons.org/licenses/by/1.0/> ;
    schema:name "VANN: A vocabulary for annotating vocabulary descriptions" ;
    schema:publisher <http://iandavis.com/id/me> ;
.

