PREFIX cc: <http://web.resource.org/cc/>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX ont: <http://purl.org/vocab/vann>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX vann: <http://purl.org/vocab/vann/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

vann:changes
    a owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf rdfs:seeAlso ;
    schema:description "A reference to a resource that describes changes between this version of a vocabulary and the previous." ;
    schema:name "Changes" ;
.

vann:example
    a owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf rdfs:seeAlso ;
    schema:description "A reference to a resource that provides an example of how this resource can be used." ;
    schema:name "Example" ;
.

vann:preferredNamespacePrefix
    a owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    schema:description "The preferred namespace prefix to use when using terms from this vocabulary in an XML document." ;
    schema:name "Preferred Namespace Prefix" ;
.

vann:preferredNamespaceUri
    a owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    schema:description "The preferred namespace URI to use when using terms from this vocabulary in an XML document." ;
    schema:name "Preferred Namespace Uri" ;
.

vann:termGroup
    a owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    schema:description "A group of related terms in a vocabulary." ;
    schema:name "Term Group" ;
.

vann:usageNote
    a owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf rdfs:seeAlso ;
    schema:description "A reference to a resource that provides information on how this resource is to be used." ;
    schema:name "Usage Note" ;
.

<http://creativecommons.org/licenses/by/1.0/>
