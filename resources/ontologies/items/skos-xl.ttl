PREFIX ont: <http://www.w3.org/2008/05/skos-xl>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX skosxl: <http://www.w3.org/2008/05/skos-xl#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

skosxl:altLabel
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:range skosxl:Label ;
    rdfs:seeAlso skos:altLabel ;
    skos:note "If C skosxl:altLabel L and L skosxl:literalForm V, then X skos:altLabel V." ;
    schema:description "The property skosxl:altLabel is used to associate an skosxl:Label with a skos:Concept. The property is analogous to skos:altLabel." ;
    schema:name "alternative label" ;
.

skosxl:hiddenLabel
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:range skosxl:Label ;
    rdfs:seeAlso skos:hiddenLabel ;
    skos:note "If C skosxl:hiddenLabel L and L skosxl:literalForm V, then C skos:hiddenLabel V." ;
    schema:description "The property skosxl:hiddenLabel is used to associate an skosxl:Label with a skos:Concept. The property is analogous to skos:hiddenLabel." ;
    schema:name "hidden label" ;
.

skosxl:labelRelation
    a
        rdf:Property ,
        owl:ObjectProperty ,
        owl:SymmetricProperty ;
    rdfs:domain skosxl:Label ;
    rdfs:isDefinedBy ont: ;
    rdfs:range skosxl:Label ;
    skos:scopeNote "skosxl:labelRelation is not intended to be used directly, but rather as the basis for a design pattern which can be refined for more specific labeling scenarios." ;
    schema:description "The property skosxl:labelRelation is used for representing binary ('direct') relations between instances of the class skosxl:Label." ;
    schema:name "label relation" ;
.

skosxl:prefLabel
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:range skosxl:Label ;
    rdfs:seeAlso skos:prefLabel ;
    skos:note "If C skosxl:prefLabel L and L skosxl:literalForm V, then X skos:prefLabel V." ;
    schema:description "The property skosxl:prefLabel is used to associate an skosxl:Label with a skos:Concept. The property is analogous to skos:prefLabel." ;
    schema:name "preferred label" ;
.

skosxl:literalForm
    a
        rdf:Property ,
        owl:DatatypeProperty ;
    rdfs:domain skosxl:Label ;
    rdfs:isDefinedBy ont: ;
    skos:note
        "If two instances of the class skosxl:Label have the same literal form, they are not necessarily the same resource." ,
        "The range of skosxl:literalForm is the class of RDF plain literals." ;
    schema:description "The property skosxl:literalForm is used to give the literal form of an skosxl:Label." ;
    schema:name "literal form" ;
.

ont:
    a owl:Ontology ;
    rdfs:seeAlso <http://www.w3.org/2008/05/skos> ;
    owl:imports <http://www.w3.org/2004/02/skos/core> ;
    skos:historyNote "2026-01 NJC: added OntPub elements, including the Ontology class definition" ;
    skos:note "Contributors: Participants in W3C's Semantic Web Deployment Working Group." ;
    schema:creator
        <https://example.com/person/Alistair-Miles> ,
        <https://example.com/person/Sean-Bechhofer> ;
    schema:dateCreated "2009-08-19"^^xsd:date ;
    schema:dateIssued "2009-08-19"^^xsd:date ;
    schema:dateModified "2009-08-19"^^xsd:date ;
    schema:description "An RDF vocabulary extending SKOS and allowing the description and linking of lexical entities." ;
    schema:name "SKOS XL Vocabulary" ;
    schema:publisher <https://www.w3.org> ;
.

skosxl:Label
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf
        [
            a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty skosxl:literalForm ;
        ] ;
    owl:disjointWith
        skos:Collection ,
        skos:Concept ,
        skos:ConceptScheme ;
    schema:description "A special class of lexical entities." ;
    schema:name "Label" ;
.
