PREFIX ont: <http://www.w3.org/2004/02/skos/core>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

skos:altLabel
    a
        rdf:Property ,
        owl:AnnotationProperty ;
    rdfs:comment
        "The range of skos:altLabel is the class of RDF plain literals." ,
        "skos:prefLabel, skos:altLabel and skos:hiddenLabel are pairwise disjoint properties." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf rdfs:label ;
    skos:example "Acronyms, abbreviations, spelling variants, and irregular plural/singular forms may be included among the alternative labels for a concept. Mis-spelled terms are normally included as hidden labels (see skos:hiddenLabel)." ;
    schema:description "An alternative lexical label for a resource." ;
    schema:name "alternative label" ;
.

skos:changeNote
    a
        rdf:Property ,
        owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf skos:note ;
    schema:description "A note about a modification to a concept." ;
    schema:name "change note" ;
.

skos:definition
    a
        rdf:Property ,
        owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf skos:note ;
    schema:description "A statement or formal explanation of the meaning of a concept." ;
    schema:name "definition" ;
.

skos:editorialNote
    a
        rdf:Property ,
        owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf skos:note ;
    schema:description "A note for an editor, translator or maintainer of the vocabulary." ;
    schema:name "editorial note" ;
.

skos:exactMatch
    a
        rdf:Property ,
        owl:ObjectProperty ,
        owl:SymmetricProperty ,
        owl:TransitiveProperty ;
    rdfs:comment "skos:exactMatch is disjoint with each of the properties skos:broadMatch and skos:relatedMatch." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf skos:closeMatch ;
    schema:description "skos:exactMatch is used to link two concepts, indicating a high degree of confidence that the concepts can be used interchangeably across a wide range of information retrieval applications. skos:exactMatch is a transitive property, and is a sub-property of skos:closeMatch." ;
    schema:name "has exact match" ;
.

skos:example
    a
        rdf:Property ,
        owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf skos:note ;
    schema:description "An example of the use of a concept." ;
    schema:name "example" ;
.

skos:hiddenLabel
    a
        rdf:Property ,
        owl:AnnotationProperty ;
    rdfs:comment
        "The range of skos:hiddenLabel is the class of RDF plain literals." ,
        "skos:prefLabel, skos:altLabel and skos:hiddenLabel are pairwise disjoint properties." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf rdfs:label ;
    schema:description "A lexical label for a resource that should be hidden when generating visual displays of the resource, but should still be accessible to free text search operations." ;
    schema:name "hidden label" ;
.

skos:historyNote
    a
        rdf:Property ,
        owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf skos:note ;
    schema:description "A note about the past state/use/meaning of a concept." ;
    schema:name "history note" ;
.

skos:member
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain skos:Collection ;
    rdfs:isDefinedBy ont: ;
    rdfs:range
        [
            owl:unionOf (
                skos:Concept
                skos:Collection
            ) ;
        ] ;
    schema:description "Relates a collection to one of its members." ;
    schema:name "has member" ;
.

skos:memberList
    a
        rdf:Property ,
        owl:FunctionalProperty ,
        owl:ObjectProperty ;
    rdfs:comment """For any resource, every item in the list given as the value of the skos:memberList property is also a value of the skos:member property.""" ;
    rdfs:domain skos:OrderedCollection ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdf:List ;
    schema:description "Relates an ordered collection to the RDF list containing its members." ;
    schema:name "has member list" ;
.

skos:notation
    a
        rdf:Property ,
        owl:DatatypeProperty ;
    rdfs:isDefinedBy ont: ;
    skos:scopeNote "By convention, skos:notation is used with a typed literal in the object position of the triple." ;
    schema:description "A notation, also known as classification code, is a string of characters such as \"T58.5\" or \"303.4833\" used to uniquely identify a concept within the scope of a given concept scheme." ;
    schema:name "notation" ;
.

skos:prefLabel
    a
        rdf:Property ,
        owl:AnnotationProperty ;
    rdfs:comment
        "A resource has no more than one value of skos:prefLabel per language tag, and no more than one value of skos:prefLabel without language tag." ,
        "The range of skos:prefLabel is the class of RDF plain literals." ,
        """skos:prefLabel, skos:altLabel and skos:hiddenLabel are pairwise
      disjoint properties.""" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf rdfs:label ;
    schema:description "The preferred lexical label for a resource, in a given language." ;
    schema:name "preferred label" ;
.

skos:relatedMatch
    a
        rdf:Property ,
        owl:ObjectProperty ,
        owl:SymmetricProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf
        skos:mappingRelation ,
        skos:related ;
    schema:description "skos:relatedMatch is used to state an associative mapping link between two conceptual resources in different concept schemes." ;
    schema:name "has related match" ;
.

skos:scopeNote
    a
        rdf:Property ,
        owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf skos:note ;
    schema:description "A note that helps to clarify the meaning and/or the use of a concept." ;
    schema:name "scope note" ;
.

skos:OrderedCollection
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf skos:Collection ;
    skos:scopeNote "Ordered collections can be used where you would like a set of concepts to be displayed in a specific order, and optionally under a 'node label'." ;
    schema:description "An ordered collection of concepts, where both the grouping and the ordering are meaningful." ;
    schema:name "Ordered Collection" ;
.

skos:broadMatch
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf
        skos:broader ,
        skos:mappingRelation ;
    owl:inverseOf skos:narrowMatch ;
    schema:description "skos:broadMatch is used to state a hierarchical mapping link between two conceptual resources in different concept schemes." ;
    schema:name "has broader match" ;
.

skos:closeMatch
    a
        rdf:Property ,
        owl:ObjectProperty ,
        owl:SymmetricProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf skos:mappingRelation ;
    schema:description "skos:closeMatch is used to link two concepts that are sufficiently similar that they can be used interchangeably in some information retrieval applications. In order to avoid the possibility of \"compound errors\" when combining mappings across more than two concept schemes, skos:closeMatch is not declared to be a transitive property." ;
    schema:name "has close match" ;
.

skos:hasTopConcept
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain skos:ConceptScheme ;
    rdfs:isDefinedBy ont: ;
    rdfs:range skos:Concept ;
    owl:inverseOf skos:topConceptOf ;
    schema:description "Relates, by convention, a concept scheme to a concept which is topmost in the broader/narrower concept hierarchies for that scheme, providing an entry point to these hierarchies." ;
    schema:name "has top concept" ;
.

skos:inScheme
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:range skos:ConceptScheme ;
    skos:scopeNote "A concept may be a member of more than one concept scheme." ;
    schema:description "Relates a resource (for example a concept) to a concept scheme in which it is included." ;
    schema:name "is in scheme" ;
.

skos:narrowMatch
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf
        skos:mappingRelation ,
        skos:narrower ;
    owl:inverseOf skos:broadMatch ;
    schema:description "skos:narrowMatch is used to state a hierarchical mapping link between two conceptual resources in different concept schemes." ;
    schema:name "has narrower match" ;
.

skos:related
    a
        rdf:Property ,
        owl:ObjectProperty ,
        owl:SymmetricProperty ;
    rdfs:comment "skos:related is disjoint with skos:broaderTransitive" ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf skos:semanticRelation ;
    schema:description "Relates a concept to a concept with which there is an associative semantic relationship." ;
    schema:name "has related" ;
.

skos:topConceptOf
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain skos:Concept ;
    rdfs:isDefinedBy ont: ;
    rdfs:range skos:ConceptScheme ;
    rdfs:subPropertyOf skos:inScheme ;
    owl:inverseOf skos:hasTopConcept ;
    schema:description "Relates a concept to the concept scheme that it is a top level concept of." ;
    schema:name "is top concept in scheme" ;
.



skos:broader
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:comment "Broader concepts are typically rendered as parents in a concept hierarchy (tree)." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf skos:broaderTransitive ;
    owl:inverseOf skos:narrower ;
    skos:scopeNote "By convention, skos:broader is only used to assert an immediate (i.e. direct) hierarchical link between two conceptual resources." ;
    schema:description "Relates a concept to a concept that is more general in meaning." ;
    schema:name "has broader" ;
.

skos:broaderTransitive
    a
        rdf:Property ,
        owl:ObjectProperty ,
        owl:TransitiveProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf skos:semanticRelation ;
    owl:inverseOf skos:narrowerTransitive ;
    skos:scopeNote "By convention, skos:broaderTransitive is not used to make assertions. Rather, the properties can be used to draw inferences about the transitive closure of the hierarchical relation, which is useful e.g. when implementing a simple query expansion algorithm in a search application." ;
    schema:description "skos:broaderTransitive is a transitive superproperty of skos:broader." ;
    schema:name "has broader transitive" ;
.

skos:narrower
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:comment "Narrower concepts are typically rendered as children in a concept hierarchy (tree)." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf skos:narrowerTransitive ;
    owl:inverseOf skos:broader ;
    skos:scopeNote "By convention, skos:broader is only used to assert an immediate (i.e. direct) hierarchical link between two conceptual resources." ;
    schema:description "Relates a concept to a concept that is more specific in meaning." ;
    schema:name "has narrower" ;
.

skos:narrowerTransitive
    a
        rdf:Property ,
        owl:ObjectProperty ,
        owl:TransitiveProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf skos:semanticRelation ;
    owl:inverseOf skos:broaderTransitive ;
    skos:scopeNote "By convention, skos:narrowerTransitive is not used to make assertions. Rather, the properties can be used to draw inferences about the transitive closure of the hierarchical relation, which is useful e.g. when implementing a simple query expansion algorithm in a search application." ;
    schema:description "skos:narrowerTransitive is a transitive superproperty of skos:narrower." ;
    schema:name "has narrower transitive" ;
.

skos:Collection
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    owl:disjointWith
        skos:Concept ,
        skos:ConceptScheme ;
    skos:scopeNote "Labelled collections can be used where you would like a set of concepts to be displayed under a 'node label' in the hierarchy." ;
    schema:description "A meaningful collection of concepts." ;
    schema:name "Collection" ;
.

skos:ConceptScheme
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    owl:disjointWith skos:Concept ;
    skos:example "Thesauri, classification schemes, subject heading lists, taxonomies, 'folksonomies', and other types of controlled vocabulary are all examples of concept schemes. Concept schemes are also embedded in glossaries and terminologies." ;
    skos:scopeNote "A concept scheme may be defined to include concepts from different sources." ;
    schema:description "A set of concepts, optionally including statements about semantic relationships between those concepts." ;
    schema:name "Concept Scheme" ;
.

skos:mappingRelation
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:comment "These concept mapping relations mirror semantic relations, and the data model defined below is similar (with the exception of skos:exactMatch) to the data model defined for semantic relations. A distinct vocabulary is provided for concept mapping relations, to provide a convenient way to differentiate links within a concept scheme from links between concept schemes. However, this pattern of usage is not a formal requirement of the SKOS data model, and relies on informal definitions of best practice." ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf skos:semanticRelation ;
    schema:description "Relates two concepts coming, by convention, from different schemes, and that have comparable meanings" ;
    schema:name "is in mapping relation with" ;
.

skos:semanticRelation
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain skos:Concept ;
    rdfs:isDefinedBy ont: ;
    rdfs:range skos:Concept ;
    skos:scopeNote "This property should not be used directly, but as a super-property for all properties denoting a relationship of meaning between concepts." ;
    schema:description "Links a concept to a concept related by meaning." ;
    schema:name "is in semantic relation with" ;
.

skos:note
    a
        rdf:Property ,
        owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    skos:scopeNote "This property may be used directly, or as a super-property for more specific note types." ;
    schema:description "A general note, for any purpose." ;
    schema:name "note" ;
.

skos:Concept
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    schema:description "An idea or notion; a unit of thought." ;
    schema:name "Concept" ;
.

ont:
    a owl:Ontology ;
    rdfs:seeAlso <http://www.w3.org/TR/skos-reference/> ;
    skos:historyNote "2026-01 NJC: added OntPub elements, including the Ontology class definition" ;
    skos:note "Contributors: Participants in W3C's Semantic Web Deployment Working Group." ;
    schema:contributor
        <https://example.com/person/Dave-Beckett> ,
        <https://example.com/person/Nikki-Rogers> ;
    schema:creator
        <https://example.com/person/Alistair-Miles> ,
        <https://example.com/person/Sean-Bechhofer> ;
    schema:dateCreated "2009-08-19"^^xsd:date ;
    schema:dateIssued "2009-08-19"^^xsd:date ;
    schema:dateModified "2026-02-13"^^xsd:date ;
    schema:description "An RDF vocabulary for describing the basic structure and content of concept schemes such as thesauri, classification schemes, subject heading lists, taxonomies, 'folksonomies', other types of controlled vocabulary, and also concept schemes embedded in glossaries and terminologies." ;
    schema:name "SKOS Vocabulary" ;
    schema:publisher <https://www.w3.org> ;
.
