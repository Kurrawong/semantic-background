    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf
        <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ,
        foaf:Agent ;
    owl:disjointWith
        foaf:Organization ,
        foaf:Project ;
    owl:equivalentClass
        <http://schema.org/Person> ,
        <http://www.w3.org/2000/10/swap/pim/contact#Person> ;
    vs:term_status "stable" ;
    schema:description "A person." ;
    schema:name "Person" ;
.

foaf:Agent
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    owl:equivalentClass dcterms:Agent ;
    vs:term_status "stable" ;
    schema:description "An agent (eg. person, group, software or physical artifact)." ;
    schema:name "Agent" ;
.

foaf:
    a owl:Ontology ;
    skos:historyNote "2026-01 NJC: added OntPub elements, including the Ontology class definition" ;
    schema:creator
        <http://example.com/person/danbri> ,
        <http://example.com/person/libby-miller> ;
    schema:dateCreated "2000"^^xsd:gYear ;
    schema:dateIssued "2014-01-14"^^xsd:date ;
    schema:dateModified "2014-01-14"^^xsd:date ;
    schema:description "The Friend of a Friend (FOAF) RDF vocabulary, described using W3C RDF Schema and the Web Ontology Language." ;
    schema:name "Friend of a Friend (FOAF) vocabulary" ;
    schema:publisher
        <http://example.com/person/danbri> ,
        <http://example.com/person/libby-miller> ;
.
