PREFIX : <https://www.ica.org/standards/RiC/ontology#>
PREFIX dc: <http://purl.org/dc/elements/1.1/>
PREFIX ont: <https://www.ica.org/standards/RiC/ontology>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX ric-dft: <https://www.ica.org/standards/RiC/vocabularies/documentaryFormTypes#>
PREFIX ric-rst: <https://www.ica.org/standards/RiC/vocabularies/recordSetTypes#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:CarrierExtent
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Extent ;
    skos:scopeNote "In order to manage an instantiation of a record resource it is necessary to note the extent of the carrier as well as that of the instantiation itself. Whether it is  necessary to note dimensions, the number of relevant units or both depends on the nature of the carrier and particular business needs." ;
    schema:description "Number of physical units and/or physical dimensions of the carrier of an Instantiation." ;
    schema:name "Carrier Extent" ;
    :RiCCMCorrespondingComponent "Class implementation of RiC-A04 (Carrier Extent) attribute" ;
.

:InstantiationExtent
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Extent ;
    schema:description "Countable characteristics of an Instantiation expressed as a quantity." ;
    schema:name "Instantiation Extent" ;
    :RiCCMCorrespondingComponent "Class implementation of RiC-A23 (Instantiation Extent attribute)" ;
.

:RecordResourceExtent
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :Extent ;
    skos:scopeNote "The method and precision of expressing the quantity of information represented in a Record Resource will vary according to the kind of record resource being  described, processing economy constraints, etc. For Record Sets, quantity may be expressed as number of records, or, for analogue records in particular, by the physical storage  dimensions of the members of the Record Set. For individual Records or Record Parts, quantity may be expressed in more precise terms." ;
    schema:description "The quantity of information content, as human experienced, contained in a Record Resource." ;
    schema:name "Record Resource Extent" ;
    :RiCCMCorrespondingComponent "Class implementation of RiC-A35 (Record Resource extent attribute)" ;
.

:RiCCMCorrespondingComponent
    a owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf rdfs:comment ;
    schema:description "When it exists, specifies the identifier and name of RiC-CM component that corresponds to the annotated class or property." ;
    schema:name "RiC-CM corresponding component" ;
.

:accruals
    a owl:DatatypeProperty ;
    rdfs:domain :RecordSet ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    skos:example
        "Accruing - there is an agreement with the creator that additional snapshots of their email directory will be accessioned at yearly intervals" ,
        "Non-accruing" ,
        "Unknown" ;
    skos:scopeNote "See also accrualsStatus" ;
    schema:description "Information on the anticipated accession(s) to the Record Set." ;
    schema:name "accruals" ;
    :RiCCMCorrespondingComponent "corresponds to RiC-A01 (Accruals attribute)" ;
.

:accrualsStatus
    a owl:DatatypeProperty ;
    rdfs:domain :RecordSet ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    skos:scopeNote "A text statement or single words such as “Closed” to indicate that no additional Record Resource will (or is anticipated to) be added to the Record Set; “Open” to indicate that additional records or record sets will (or are expected to) be added to the Record Set; or “Unknown” to indicate that this information is not available,  for example. See also accruals" ;
    schema:description "Information on the status of possible accruals" ;
    schema:name "accruals status" ;
    :RiCCMCorrespondingComponent "specialization of RiC-A01 (Accruals attribute)" ;
.

:altimetricSystem
    a owl:DatatypeProperty ;
    rdfs:domain :Coordinates ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf :referenceSystem ;
    schema:description "Reference system used for altitude" ;
    schema:name "altimetric system" ;
.

:altitude
    a owl:DatatypeProperty ;
    rdfs:domain :Coordinates ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf :measure ;
    skos:scopeNote "Property of the Coordinates class. If you don't use this class, use geographicalCoordinates property, a property of Place class." ;
    schema:description "The height of a Place above a reference level, especially above sea level." ;
    schema:name "altitude" ;
.

:authenticityNote
    a owl:DatatypeProperty ;
    rdfs:domain
        [
            owl:unionOf (
                :Instantiation
                :RecordResource
            ) ;
        ] ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    skos:example
        "The deed is digitally signed by the Notary. The electronic signature validity cannot be assessed, but the content was not modified from the moment of signing." ,
        "The letter is unsigned." ,
        "The record bears signatures." ,
        "The timestamp exists but cannot be verified." ;
    skos:scopeNote "May be used in a Record Set description when its value is shared by some or all members of the Record Set. For digital records, it may include results from automated  means of checking the validity of signatures and timestamp. In particular cases it may be contextually related to the state attribute, for example, a document can be an original or a  copy, either of which can be authentic or a forgery. " ;
    schema:description "Description of the evidence that a Record Resource or Instantiation is what it purports to be, was created or sent by the said Agent at the said time, and has not been tampered with, corrupted, or forged. " ;
    schema:name "authenticity note" ;
    :RiCCMCorrespondingComponent "Corresponds to RiC-A03 (Authenticity Note attribute)" ;
.

:authorizingMandate
    a owl:DatatypeProperty ;
    rdfs:domain :Agent ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf :ruleFollowed ;
    skos:scopeNote "Provided for usability reasons. May be deprecated and removed later on. Use only if you don't use the Mandate class for handling mandates." ;
    schema:description "Information on a Mandate that authorizes an Agent to perform an Activity." ;
    schema:name "authorizing mandate" ;
.

:birthDate
    a owl:DatatypeProperty ;
    rdfs:domain :Person ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf :beginningDate ;
    skos:scopeNote "Provided for usability reasons. May be deprecated and removed later on. Use only if you don't use Date classes for handling dates." ;
    schema:description "Date at which a Person was born." ;
    schema:name "birth date" ;
    :RiCCMCorrespondingComponent "Datatype property specialized implementation of RiC-E18 (Date entity)" ;
.

:carrierExtent
    a owl:DatatypeProperty ;
    rdfs:domain :Instantiation ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf :physicalOrLogicalExtent ;
    skos:example
        "1 page" ,
        "3 GB (1 USB key)" ,
        "32.5 x 49 cm (piece of parchment)" ;
    skos:scopeNote "For digital resources, it may be used to indicate the size of storage capacity (disk, tape, film, etc.). Carrier extent should not be confused with instantiation  extent or record resource extent. For a given Record Resource, the instantiation extent may vary, based on format, density of information on the carrier, etc. For example, a CD with a  storage capacity of 700 MB (carrier extent) might hold a record of 1500 words (record resource extent) represented in two versions, one a Word document with an instantiation extent of 3 KB and the other a PDF file with an instantiation extent  of 5 KB. " ;
    schema:description "Number of physical units and/or physical dimensions of the carrier of an Instantiation. In order to manage an Instantiation of a record resource it is necessary  to note the extent of the carrier as well as that of the Instantiation itself. Whether it is necessary to note dimensions, the number of relevant units, or both, depends on the nature of the carrier and particular business needs." ;
    schema:name "carrier extent" ;
    :RiCCMCorrespondingComponent "Corresponds to RiC-A04 (Carrier Extent attribute)" ;
.

:classification
    a owl:DatatypeProperty ;
    rdfs:domain :RecordResource ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    skos:example
        "BUD-01-F002 (classification number from a corporate file plan)" ,
        "digitized items" ,
        "financial affairs" ,
        "human resource management" ,
        "student registration" ;
    skos:scopeNote "May be used in a Record Set description when its value is shared by some or all members of the record set. This datatype property is not to be confused with Identifier although, in some cases, the information may be the same." ;
    schema:description "A term, number or alphanumeric string that is usually taken from an external classification vocabulary or scheme that qualifies a Record Resource. " ;
    schema:name "classification" ;
    :RiCCMCorrespondingComponent "Corresponds to RiC-A07 (Classification attribute)" ;
.

:closeTo
    a owl:AnnotationProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:subPropertyOf rdfs:comment ;
    schema:description "An annotation property for recording a possible mapping to a component in another model or ontology" ;
    schema:name "close to" ;
.

:conditionsOfAccess
    a owl:DatatypeProperty ;
    rdfs:domain
        [
            owl:unionOf (
                :Instantiation
                :RecordResource
            ) ;
        ] ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    skos:example
        "Closed as awaiting conservation treatment" ,
        "Closed for 30 years" ,
        "Closed under data protection legislation" ,
        "Open" ,
        "Recognita software, min. version 3.0, is needed in order to open the file" ,
        "The Archives cannot provide VHS reader to access the content of the tape" ,
        "Acceso libre a través de los terminales de consulta"@es ;
    skos:scopeNote "May be used in a Record Set description when its value is shared by some or all members of the Record Set. The attribute provides information about the accessibility  of a Record Resource, as well as the physical, technical or legal limitations that exist for providing access to it." ;
    schema:description "Terms and circumstances affecting the availability of a Record Resource or an Instantiation for consultation." ;
    schema:name "conditions of access" ;
    :RiCCMCorrespondingComponent "Corresponds to RiC-A08 (Conditions of Access attribute)" ;
.

:conditionsOfUse
    a owl:DatatypeProperty ;
    rdfs:domain
        [
            owl:unionOf (
                :Instantiation
                :RecordResource
            ) ;
        ] ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    skos:example
        "Cannot be copied using warm light copying machines or photographed using flashlight" ,
        "Permission of the copyright owner must be obtained before use" ;
    skos:scopeNote "May be used in a Record Set description when its value is shared by some or all members of the Record Set." ;
    schema:description "Terms and circumstances affecting the use of a Record Resource or an Instantiation after access has been provided. Includes conditions governing reproduction of  the Record Resource under applicable copyright (intellectual property) and/or property legislation or due to conservation status." ;
    schema:name "conditions of use" ;
    :RiCCMCorrespondingComponent "Corresponds to RiC-A09 (Conditions of Use attribute)" ;
.

:creationDate
    a owl:DatatypeProperty ;
    rdfs:domain :Thing ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf :beginningDate ;
    skos:scopeNote "Provided for usability reasons. May be deprecated and removed later on. Use only if you don't use Date classes for handling dates." ;
    schema:description "Date at which an entity was created." ;
    schema:name "creation date" ;
    :RiCCMCorrespondingComponent "Datatype property specialized implementation of RiC-E18 (Date entity)" ;
.

:dateQualifier
    a owl:DatatypeProperty ;
    rdfs:domain :Date ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    skos:example
        "approximate" ,
        "circa" ,
        "end unknown" ;
    skos:scopeNote "Most often, this human readable expression of the accuracy of the date is used along with an ISO 8601 representation of the date. The qualifier can also be expressed  as a machine-readable value, using the EDTF standard (thus, a specialization of the normalizedDateValue datatype property)." ;
    schema:description "A human readable qualification of a Date to indicate the level of precision or certainty." ;
    schema:name "date qualifier" ;
    :RiCCMCorrespondingComponent "Corresponds to RiC-A13 (Date Qualifier attribute)" ;
.

:deathDate
    a owl:DatatypeProperty ;
    rdfs:domain :Person ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf :endDate ;
    skos:scopeNote "Provided for usability reasons. May be deprecated and removed later on. Use only if you don't use Date classes for handling dates." ;
    schema:description "Date at which a Person died." ;
    schema:name "death date" ;
    :RiCCMCorrespondingComponent "Datatype property specialized implementation of RiC-E18 (Date entity)" ;
.

:destructionDate
    a owl:DatatypeProperty ;
    rdfs:domain :Thing ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf :endDate ;
    skos:scopeNote "Provided for usability reasons. May be deprecated and removed later on. Use only if you don't use Date classes for handling dates." ;
    schema:description "Date at which an entity was deleted." ;
    schema:name "destruction date" ;
    :RiCCMCorrespondingComponent "Datatype property specialized implementation of RiC-E18 (Date entity)" ;
.

:expressedDate
    a owl:DatatypeProperty ;
    rdfs:domain :Date ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf
        :name ,
        :textualValue ;
    skos:example
        "1550-1553, 1555 (date range)" ,
        "1925-1966 (date range)" ,
        "All of the years 1550, 1551, 1553, 1555" ,
        "October 24, 1999 (month day, year)" ,
        "One of the years 1550, 1551, 1553, 1555" ,
        "The Middle Ages" ,
        "The second semester of 1951 to 1952 (academic reference system)" ,
        "15 thermidor an IV (calendrier révolutionnaire français)"@fr ,
        "Die jovis ultima mensis martii anno domini millesimo quingentesimo quadragesimo ante Pascha"@la ;
    schema:description "Natural language expression of a date. This property is a specialization of the name property. In order that the precise meaning of the date can be understood,  information such as the calendar used or other specific context should be included." ;
    schema:name "expressed date" ;
    :RiCCMCorrespondingComponent "Corresponds to RiC-A19 (Expressed Date attribute)" ;
.

:geodesicSystem
    a owl:DatatypeProperty ;
    rdfs:domain :Coordinates ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf :referenceSystem ;
    schema:description "Reference system used for geographical coordinates." ;
    schema:name "geodesic system" ;
.

:geographicalCoordinates
    a owl:DatatypeProperty ;
    rdfs:domain :Place ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    skos:example
        """Latitude 35.89421911, Longitude 139.94637467 (ISO
         6709/F)""" ,
        "Latitude 50°40′46,461″N, Longitude 95°48′26,533″W, Height 123,45m (ISO 6709/D)" ;
    skos:scopeNote "Provided for usability reasons. May be deprecated and removed later on. Use only if you don't use PhysicalLocation and Coordinates classes with Place. Coordinates may be based on ISO 6709 Standard representation of geographic point location by coordinates." ;
    schema:description "Longitudinal and latitudinal information about a Place. " ;
    schema:name "geographical coordinates" ;
    :RiCCMCorrespondingComponent "Corresponds to RiC-A11 (Coordinates attribute)" ;
.

:hasOrHadCorrespondent
    a
        owl:ObjectProperty ,
        owl:SymmetricProperty ;
    rdfs:domain :Person ;
    rdfs:isDefinedBy ont: ;
    rdfs:range :Person ;
    rdfs:subPropertyOf :knows ;
    owl:propertyChainAxiom (
        :thingIsConnectedToRelation
        :correspondenceRelation_role
        :relationConnects
    ) ;
    schema:description "Connects two Persons that correspond or have corresponded with each other. This relation is symmetric." ;
    schema:name "has or had correspondent" ;
    :RiCCMCorrespondingComponent "RiC-R052 ('has or had correspondent' relation)" ;
.

:hasOrHadSpouse
    a
        owl:ObjectProperty ,
        owl:SymmetricProperty ;
    rdfs:domain :Person ;
    rdfs:isDefinedBy ont: ;
    rdfs:range :Person ;
    rdfs:subPropertyOf :hasFamilyAssociationWith ;
    owl:propertyChainAxiom (
        :thingIsConnectedToRelation
        :spouseRelation_role
        :relationConnects
    ) ;
    schema:description "Connects two Persons who are or were married. This relation is symmetric." ;
    schema:name "has or had spouse" ;
    :RiCCMCorrespondingComponent "RiC-R049 ('has or had spouse' relation)" ;
.

:hasOrHadWorkRelationWith
    a
        owl:ObjectProperty ,
        owl:SymmetricProperty ;
    rdfs:domain :Agent ;
    rdfs:isDefinedBy ont: ;
    rdfs:range :Agent ;
    rdfs:subPropertyOf :isAgentAssociatedWithAgent ;
    owl:propertyChainAxiom (
        :thingIsConnectedToRelation
        :workRelation_role
        :relationConnects
    ) ;
