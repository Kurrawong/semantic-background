    schema:name "has or had work relation with" ;
    :RiCCMCorrespondingComponent "RiC-R046 ('has or had work relation with' relation)" ;
.

:hasSibling
    a
        owl:ObjectProperty ,
        owl:SymmetricProperty ;
    rdfs:domain :Person ;
    rdfs:isDefinedBy ont: ;
    rdfs:range :Person ;
    rdfs:subPropertyOf :hasFamilyAssociationWith ;
    owl:propertyChainAxiom (
        :thingIsConnectedToRelation
        :siblingRelation_role
        :relationConnects
    ) ;
    schema:description "Connects two Persons who are siblings. This relation is symmetric." ;
    schema:name "has sibling" ;
    :RiCCMCorrespondingComponent "RiC-R048 ('has sibling' relation)" ;
