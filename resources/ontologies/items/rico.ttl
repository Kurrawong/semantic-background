:height
    a owl:DatatypeProperty ;
    rdfs:domain :Thing ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    rdfs:subPropertyOf :measure ;
    schema:description "Vertical dimension of an entity." ;
