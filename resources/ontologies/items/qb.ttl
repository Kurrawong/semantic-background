PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX ont: <http://purl.org/linked-data/cube>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX qb: <http://purl.org/linked-data/cube#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX scovo: <http://purl.org/NET/scovo#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

qb:attribute
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:range qb:AttributeProperty ;
    rdfs:subPropertyOf qb:componentProperty ;
    schema:description "An alternative to qb:componentProperty which makes explicit that the component is a attribute" ;
    schema:name "attribute" ;
.

qb:codeList
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain qb:CodedProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:range
        [
            owl:unionOf (
                skos:ConceptScheme
                skos:Collection
                qb:HierarchicalCodeList
            ) ;
        ] ;
    schema:description "gives the code list associated with a CodedProperty" ;
    schema:name "code list" ;
.

qb:component
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain qb:DataStructureDefinition ;
    rdfs:isDefinedBy ont: ;
    rdfs:range qb:ComponentSpecification ;
    schema:description "indicates a component specification which is included in the structure of the dataset" ;
    schema:name "component specification" ;
.

qb:componentAttachment
    a rdf:Property ;
    rdfs:domain qb:ComponentSpecification ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Class ;
    schema:description "Indicates the level at which the component property should be attached, this might an qb:DataSet, qb:Slice or qb:Observation, or a qb:MeasureProperty." ;
    schema:name "component attachment" ;
.

qb:componentRequired
    a
        rdf:Property ,
        owl:DatatypeProperty ;
    rdfs:domain qb:ComponentSpecification ;
    rdfs:isDefinedBy ont: ;
    rdfs:range xsd:boolean ;
    schema:description """Indicates whether a component property is required (true) or optional (false) in the context of a DSD. Only applicable
    to components correspond to an attribute. Defaults to false (optional).""" ;
    schema:name "component required" ;
.

qb:concept
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain qb:ComponentProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:range skos:Concept ;
    schema:description "gives the concept which is being measured or indicated by a ComponentProperty" ;
    schema:name "concept" ;
.

qb:dataSet
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain qb:Observation ;
    rdfs:isDefinedBy ont: ;
    rdfs:range qb:DataSet ;
    owl:equivalentProperty scovo:dataset ;
    schema:description "indicates the data set of which this observation is a part" ;
    schema:name "data set" ;
.

qb:dimension
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:range qb:DimensionProperty ;
    rdfs:subPropertyOf qb:componentProperty ;
    schema:description "An alternative to qb:componentProperty which makes explicit that the component is a dimension" ;
    schema:name "dimension" ;
.

qb:hierarchyRoot
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain qb:HierarchicalCodeList ;
    rdfs:isDefinedBy ont: ;
    schema:description "Specifies a root of the hierarchy. A hierarchy may have multiple roots but must have at least one." ;
    schema:name "hierarchy root" ;
.

qb:measure
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:range qb:MeasureProperty ;
    rdfs:subPropertyOf qb:componentProperty ;
    schema:description "An alternative to qb:componentProperty which makes explicit that the component is a measure" ;
    schema:name "measure" ;
.

qb:measureDimension
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:range qb:DimensionProperty ;
    rdfs:subPropertyOf qb:componentProperty ;
    schema:description "An alternative to qb:componentProperty which makes explicit that the component is a measure dimension" ;
    schema:name "measure dimension" ;
.

qb:measureType
    a
        qb:DimensionProperty ,
        rdf:Property ;
    rdfs:isDefinedBy ont: ;
    rdfs:range qb:MeasureProperty ;
    schema:description "Generic measure dimension, the value of this dimension indicates which measure (from the set of measures in the DSD) is being given by the obsValue (or other primary measure)" ;
    schema:name "measure type" ;
.

qb:observation
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain qb:ObservationGroup ;
    rdfs:isDefinedBy ont: ;
    rdfs:range qb:Observation ;
    schema:description "indicates a observation contained within this slice of the data set" ;
    schema:name "observation" ;
.

qb:order
    a
        rdf:Property ,
        owl:DatatypeProperty ;
    rdfs:domain qb:ComponentSpecification ;
    rdfs:isDefinedBy ont: ;
    rdfs:range xsd:int ;
    schema:description "indicates a priority order for the components of sets with this structure, used to guide presentations - lower order numbers come before higher numbers, un-numbered components come last" ;
    schema:name "order" ;
.

qb:parentChildProperty
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain qb:HierarchicalCodeList ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdf:Property ;
    schema:description "Specifies a property which relates a parent concept in the hierarchy to a child concept." ;
    schema:name "parent-child property" ;
.

qb:slice
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain qb:DataSet ;
    rdfs:isDefinedBy ont: ;
    rdfs:range qb:Slice ;
    rdfs:subPropertyOf qb:observationGroup ;
    schema:description "Indicates a subset of a DataSet defined by fixing a subset of the dimensional values" ;
    schema:name "slice" ;
.

qb:sliceKey
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain qb:DataStructureDefinition ;
    rdfs:isDefinedBy ont: ;
    rdfs:range qb:SliceKey ;
    schema:description "indicates a slice key which is used for slices in this dataset" ;
    schema:name "slice key" ;
.

qb:sliceStructure
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain qb:Slice ;
    rdfs:isDefinedBy ont: ;
    rdfs:range qb:SliceKey ;
    schema:description "indicates the sub-key corresponding to this slice" ;
    schema:name "slice structure" ;
.

qb:structure
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain qb:DataSet ;
    rdfs:isDefinedBy ont: ;
    rdfs:range qb:DataStructureDefinition ;
    schema:description "indicates the structure to which this data set conforms" ;
    schema:name "structure" ;
.

qb:observationGroup
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:isDefinedBy ont: ;
    rdfs:range qb:ObservationGroup ;
    schema:description "Indicates a group of observations. The domain of this property is left open so that a group may be attached to different resources and need not be restricted to a single DataSet" ;
    schema:name "observation group" ;
.

