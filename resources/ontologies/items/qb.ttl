.

qb:AttributeProperty
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf qb:ComponentProperty ;
    owl:disjointWith qb:MeasureProperty ;
    schema:description "The class of components which represent attributes of observations in the cube, e.g. unit of measurement" ;
    schema:name "Attribute property" ;
.

qb:CodedProperty
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf qb:ComponentProperty ;
    schema:description "Superclass of all coded ComponentProperties" ;
    schema:name "Coded property" ;
.

qb:Observation
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf qb:Attachable ;
    owl:equivalentClass scovo:Item ;
    schema:description "A single observation in the cube, may have one or more associated measured values" ;
    schema:name "Observation" ;
.

qb:Slice
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf
        qb:Attachable ,
        qb:ObservationGroup ;
    schema:description "Denotes a subset of a DataSet defined by fixing a subset of the dimensional values, component properties on the Slice" ;
    schema:name "Slice" ;
.

qb:SliceKey
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf qb:ComponentSet ;
    schema:description "Denotes a subset of the component properties of a DataSet which are fixed in the corresponding slices" ;
    schema:name "Slice key" ;
.

qb:Attachable
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    schema:description "Abstract superclass for everything that can have attributes and dimensions" ;
    schema:name "Attachable (abstract)" ;
.

qb:DataSet
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf qb:Attachable ;
    owl:equivalentClass scovo:Dataset ;
    schema:description "Represents a collection of observations, possibly organized into various slices, conforming to some common dimensional structure." ;
    schema:name "Data set" ;
.

qb:DataStructureDefinition
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf qb:ComponentSet ;
    schema:description "Defines the structure of a DataSet or slice" ;
    schema:name "Data structure definition" ;
.

qb:HierarchicalCodeList
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    schema:description "Represents a generalized hierarchy of concepts which can be used for coding. The hierarchy is defined by one or more roots together with a property which relates concepts in the hierarchy to thier child concept .  The same concepts may be members of multiple hierarchies provided that different qb:parentChildProperty values are used for each hierarchy." ;
    schema:name "Hierarchical Code List" ;
.

qb:ObservationGroup
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    schema:description "A, possibly arbitrary, group of observations." ;
    schema:name "Observation Group" ;
.

qb:ComponentSet
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    schema:description "Abstract class of things which reference one or more ComponentProperties" ;
    schema:name "Component set" ;
.

qb:ComponentSpecification
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf qb:ComponentSet ;
    schema:description "Used to define properties of a component (attribute, dimension etc) which are specific to its usage in a DSD." ;
    schema:name "Component specification" ;
.

qb:DimensionProperty
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf
        qb:CodedProperty ,
        qb:ComponentProperty ;
    owl:disjointWith qb:MeasureProperty ;
    schema:description "The class of components which represent the dimensions of the cube" ;
    schema:name "Dimension property" ;
.

qb:MeasureProperty
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf qb:ComponentProperty ;
    owl:disjointWith
        qb:AttributeProperty ,
        qb:DimensionProperty ;
    schema:description "The class of components which represent the measured value of the phenomenon being observed" ;
    schema:name "Measure property" ;
.

qb:componentProperty
    a
        rdf:Property ,
        owl:ObjectProperty ;
    rdfs:domain qb:ComponentSet ;
    rdfs:isDefinedBy ont: ;
    rdfs:range qb:ComponentProperty ;
    schema:description "indicates a ComponentProperty (i.e. attribute/dimension) expected on a DataSet, or a dimension fixed in a SliceKey" ;
    schema:name "component" ;
.

qb:ComponentProperty
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf rdf:Property ;
    schema:description "Abstract super-property of all properties representing dimensions, attributes or measures" ;
    schema:name "Component property (abstract)" ;
.

