PREFIX ont: <http://www.w3.org/2000/01/rdf-schema>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

rdfs:Class
    a rdfs:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf rdfs:Resource ;
    schema:description "The class of classes." ;
    schema:name "Class" ;
.

rdfs:Container
    a rdfs:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf rdfs:Resource ;
    schema:description "The class of RDF containers." ;
    schema:name "Container" ;
.

rdfs:ContainerMembershipProperty
    a rdfs:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf rdf:Property ;
    schema:description "The class of container membership properties, rdf:_1, rdf:_2, ..., all of which are sub-properties of 'member'." ;
    schema:name "Container Membership Property" ;
.

rdfs:Datatype
    a rdfs:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf rdfs:Class ;
    schema:description "The class of RDF datatypes." ;
    schema:name "Datatype" ;
.

rdfs:Literal
    a rdfs:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf rdfs:Resource ;
    schema:description "The class of literal values, eg. textual strings and integers." ;
    schema:name "Literal" ;
.

rdfs:Resource
    a rdfs:Class ;
    rdfs:isDefinedBy ont: ;
    schema:description "The class resource, everything." ;
    schema:name "Resource" ;
.

rdfs:comment
    a rdf:Property ;
    rdfs:domain rdfs:Resource ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    schema:description "A description of the subject resource." ;
    schema:name "comment" ;
.

rdfs:domain
    a rdf:Property ;
    rdfs:domain rdf:Property ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Class ;
    schema:description "A domain of the subject property." ;
    schema:name "domain" ;
.

rdfs:isDefinedBy
    a rdf:Property ;
    rdfs:domain rdfs:Resource ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Resource ;
    rdfs:subPropertyOf rdfs:seeAlso ;
    schema:description "The defininition of the subject resource." ;
    schema:name "is defined by" ;
.

rdfs:label
    a rdf:Property ;
    rdfs:domain rdfs:Resource ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Literal ;
    schema:description "A human-readable name for the subject." ;
    schema:name "label" ;
.

rdfs:member
    a rdf:Property ;
    rdfs:domain rdfs:Resource ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Resource ;
    schema:description "A member of the subject resource." ;
    schema:name "member" ;
.

rdfs:range
    a rdf:Property ;
    rdfs:domain rdf:Property ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Class ;
    schema:description "A range of the subject property." ;
    schema:name "range" ;
.

rdfs:subClassOf
    a rdf:Property ;
    rdfs:domain rdfs:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Class ;
    schema:description "The subject is a subclass of a class." ;
    schema:name "subclass of" ;
.

rdfs:subPropertyOf
    a rdf:Property ;
    rdfs:domain rdf:Property ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdf:Property ;
    schema:description "The subject is a subproperty of a property." ;
    schema:name "subproperty of" ;
.

rdfs:seeAlso
    a rdf:Property ;
    rdfs:domain rdfs:Resource ;
    rdfs:isDefinedBy ont: ;
    rdfs:range rdfs:Resource ;
    schema:description "Further information about the subject resource." ;
    schema:name "see also" ;
.

ont:
    a owl:Ontology ;
    rdfs:seeAlso ont:-more ;
    skos:historyNote "2026-01 NJC: added OntPub elements, including the Ontology class definition" ;
    schema:creator <https://www.w3.org> ;
    schema:dateCreated "2014-02-25"^^xsd:date ;
    schema:dateIssued "2014-02-25"^^xsd:date ;
    schema:dateModified "2023-12-01"^^xsd:date ;
    schema:description "The RDF Schema vocabulary (RDFS)" ;
    schema:name "The RDF Schema vocabulary (RDFS)" ;
    schema:publisher <https://www.w3.org> ;
.
