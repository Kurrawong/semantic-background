:DurationDescription
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty :years ;
        ] ,
        [
            a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty :minutes ;
        ] ,
        [
            a owl:Restriction ;
            owl:hasValue <http://www.opengis.net/def/uom/ISO-8601/0/Gregorian> ;
            owl:onProperty :hasTRS ;
        ] ,
        [
            a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty :weeks ;
        ] ,
        [
            a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty :seconds ;
        ] ,
        [
            a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty :months ;
        ] ,
        [
            a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty :hours ;
        ] ,
        [
            a owl:Restriction ;
            owl:allValuesFrom xsd:decimal ;
            owl:onProperty :days ;
        ] ,
        :GeneralDurationDescription ;
    skos:note "In the Gregorian calendar the length of the month is not fixed. Therefore, a value like \"2.5 months\" cannot be exactly compared with a similar duration expressed in terms of weeks or days." ;
    schema:description "Description of temporal extent structured with separate values for the various elements of a calendar-clock system. The temporal reference system is fixed to Gregorian Calendar, and the range of each of the numeric properties is restricted to xsd:decimal" ;
    schema:name "Duration description" ;
.

