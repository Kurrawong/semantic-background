        ] ,
        [
            a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty :unitType ;
        ] ,
        :TemporalDuration ;
    skos:note "Alternative to time:DurationDescription to support description of a temporal duration other than using a calendar/clock system." ;
    schema:description "Duration of a temporal extent expressed as a number scaled by a temporal unit" ;
    schema:name "Time duration" ;
.

:Interval
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf :TemporalEntity ;
    schema:description "A temporal entity with an extent or duration" ;
    schema:name "Time interval" ;
.

:TimePosition
    a owl:Class ;
    rdfs:isDefinedBy ont: ;
    rdfs:subClassOf 
        [
            owl:unionOf (
                
                [
                    a owl:Restriction ;
                    owl:cardinality "1"^^xsd:nonNegativeInteger ;
                    owl:onProperty :numericPosition ;
                ]
                
                [
                    a owl:Restriction ;
                    owl:cardinality "1"^^xsd:nonNegativeInteger ;
                    owl:onProperty :nominalPosition ;
                ]
            ) ;
        ] ,
        :TemporalPosition ;
    schema:description "A temporal position described using either a (nominal) value from an ordinal reference system, or a (numeric) value in a temporal coordinate system. " ;
    schema:name "Time position" ;
.

:day
    a owl:DatatypeProperty ;
    rdfs:comment """Day position in a calendar-clock system.

The range of this property is not specified, so can be replaced by any specific representation of a calendar day from any calendar. """ ;
    rdfs:domain :GeneralDateTimeDescription ;
    rdfs:isDefinedBy ont: ;
    schema:description """Day position in a calendar-clock system.

The range of this property is not specified, so can be replaced by any specific representation of a calendar day from any calendar. """ ;
    schema:name "day" ;
.

