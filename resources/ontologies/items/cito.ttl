    schema:description "A citation in which the citing and the cited entities have something significant in common with one another, over and beyond their subject matter, for example authors, journal, institutional affiliation, or funding agency." ;
    schema:name "self citation" ;
.

:cites
    a owl:ObjectProperty ;
