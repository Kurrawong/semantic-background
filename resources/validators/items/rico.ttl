:AccumulationRelationShape
    a sh:NodeShape ;
    rdfs:isDefinedBy : ;
    sh:property 
        [
            a sh:PropertyShape ;
            sh:class rico:AccumulationRelation ;
            sh:message """
- The object of the rico:accumulationRelation_role property on a rico:AccumulationRelation must be a rico:AccumulationRelation""" ;
            sh:path rico:accumulationRelation_role ;
            sh:severity sh:Warning ;
        ] ,
        [
            a sh:PropertyShape ;
            sh:message """
- A rico:AccumulationRelation must have at least 1 rico:relationHasSource statements
- The object of the rico:relationHasSource property on a rico:AccumulationRelation must be one of ['rico:Instantiation', 'rico:RecordResource']""" ;
            sh:minCount "1"^^xsd:nonNegativeInteger ;
            sh:or (
                
                [
                    sh:class rico:Instantiation ;
                ]
                
                [
                    sh:class rico:RecordResource ;
                ]
            ) ;
            sh:path rico:relationHasSource ;
            sh:severity sh:Violation ;
        ] ,
        [
            a sh:PropertyShape ;
            sh:class rico:Agent ;
            sh:message """
- A rico:AccumulationRelation must have at least 1 rico:relationHasTarget statements
- The object of the rico:relationHasTarget property on a rico:AccumulationRelation must be a rico:Agent""" ;
            sh:minCount "1"^^xsd:nonNegativeInteger ;
            sh:path rico:relationHasTarget ;
            sh:severity sh:Violation ;
        ] ;
    sh:targetClass rico:AccumulationRelation ;
.

